      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{BB7A5518-49D7-4918-BA38-AF71FB377BFB}"
          SQLTask:SqlStatementSource="INSERT INTO dbo.Hierarchy&#xA;    ([RVPRODId],&#xA;     [RVPRODStoreId],&#xA;     [RVPId],&#xA;     [RVPDesc],&#xA;     [RPPIdAndDesc],&#xA;     [RVPBeginDate],&#xA;     [RVPEndDate],&#xA;     [RODId],&#xA;     [ROD],&#xA;     [RODIdAndDesc],&#xA;     [RODBeginDate],&#xA;     [RODEndDate],&#xA;     [GeneralManager],&#xA;     [GMBeginDate],&#xA;     [GMEndDate],&#xA;     [Restaurant],&#xA;     [Category],&#xA;     [StoreName],&#xA;     [StoreIdAndName],&#xA;     [RegionID],&#xA;     [RegionName],&#xA;     [Address],&#xA;     [City],&#xA;     [State],&#xA;     [Zip],&#xA;     [OpenDate],&#xA;     [ClosedDate],&#xA;     [Telephone],&#xA;     [TimeZone],&#xA;     [SquareFootage],&#xA;     [LastUpdateDateTime],&#xA;     [Company])&#xA;SELECT DISTINCT&#xA;     LTRIM(RTRIM(CONVERT(VARCHAR(10), a.DivisionID))) + &#xA;          LTRIM(RTRIM(CONVERT(VARCHAR(10), a.RegionId))) AS 'RVPRODId',&#xA;     LTRIM(RTRIM(CONVERT(VARCHAR(10), a.DivisionID))) + &#xA;          LTRIM(RTRIM(CONVERT(VARCHAR(10), a.RegionId))) + &#xA;          LTRIM(RTRIM(CONVERT(VARCHAR(10), a.StoreId))) AS 'RVPRODStoreId',&#xA;     a.DivisionID AS 'RVPId',&#xA;     a.RegionalVicePresident AS 'RVPDesc',&#xA;     CONVERT(VARCHAR(3), a.DivisionId) + ' ' + a.RegionalVicePresident AS 'RODIdAndDesc',&#xA;     '' AS 'RVPBeginDate',&#xA;     '' AS 'RVPEndDate',&#xA;     a.RegionId AS 'RODId',&#xA;     a.RegionalOperationsDirector AS 'ROD',&#xA;     CONVERT(VARCHAR(3), a.RegionId) + ' ' + a.RegionalOperationsDirector AS 'RODIdAndDesc',&#xA;     '' AS 'RODBeginDate',&#xA;     '' AS 'RODEndDate',&#xA;     a.GeneralManager,&#xA;     '' AS 'GMBeginDate',&#xA;     '' AS 'GMEndDate',&#xA;     a.StoreID AS 'Restaurant',&#xA;     LTRIM(RTRIM(a.Category)) AS 'Category',&#xA;     a.StoreName,&#xA;     CONVERT(VARCHAR(3), a.StoreID) + ' ' + a.StoreName AS 'StoreIDAndName',&#xA;     a.RegionID,&#xA;     a.RegionName,&#xA;     a.[Address],&#xA;     a.City,&#xA;     a.[State],&#xA;     a.Zip,&#xA;     a.OpenDate,&#xA;     a.ClosedDate,&#xA;     a.Telephone,&#xA;     a.TimeZone,&#xA;     a.SquareFootage,&#xA;     CURRENT_TIMESTAMP AS 'LastUpdateDateTime',&#xA;     a.Company &#xA;FROM Staging.dbo.vwStoreAllwfp a&#xA;WHERE a.Category IS NOT NULL&#xA;     AND a.Company IN ('RRBW', 'RRGB')&#xA;ORDER BY&#xA;     a.DivisionID,&#xA;     a.StoreID" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
