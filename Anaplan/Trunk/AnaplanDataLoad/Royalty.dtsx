                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @Hierarchy TABLE (StoreID INT, Company VARCHAR(100), OpenDate DATETIME, UNIQUE CLUSTERED (StoreID, Company))
INSERT INTO @Hierarchy
SELECT Restaurant, Company, OpenDate
FROM dbo.Hierarchy
WHERE Company IN ('RRGB','RRBW') 


SELECT r.*
FROM dbo.Royalty r
INNER JOIN @Hierarchy h
ON r.Restaurant = h.StoreID
WHERE DATEADD(d, 0, DATEDIFF(d, 0, DateOfBusiness)) IN 
		(SELECT FullDate
		FROM Staging.dbo.vwCalendarShort
		WHERE Period IN 
			(SELECT Period
			FROM Staging.dbo.vwCalendarShort
			WHERE CONVERT(VARCHAR(10), FullDate, 120) = CONVERT(VARCHAR(10), CURRENT_TIMESTAMP -1, 120))
		AND FiscalYear IN 
			(SELECT FiscalYear
			FROM Staging.dbo.vwCalendarShort
			WHERE CONVERT(VARCHAR(10), FullDate, 120)  = CONVERT(VARCHAR(10), CURRENT_TIMESTAMP -1, 120))
		AND FullDate &lt;  CONVERT(VARCHAR(10), CURRENT_TIMESTAMP, 120))	
AND DateOfBusiness &gt;= h.OpenDate
ORDER BY Restaurant
,DateOfBusiness</property>
