                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @Yesterday VARCHAR(10), @Today VARCHAR(10)
SET @Yesterday = CONVERT(VARCHAR(10), CURRENT_TIMESTAMP -1, 120)
SET @Today = CONVERT(VARCHAR(10), CURRENT_TIMESTAMP, 120)

-- Limit Hierarchy to non-franchise restaurants
DECLARE @Hierarchy TABLE (StoreID INT
	, Company VARCHAR(100)
	, OpenDate DATETIME
	, UNIQUE CLUSTERED (StoreID, Company))
INSERT INTO @Hierarchy
SELECT Restaurant, Company, OpenDate
FROM [HQ-FinSQL-01].AnaplanScorecardBudget.dbo.Hierarchy
WHERE Company IN ('RRGB','RRBW') 

DECLARE @Calendar TABLE (FiscalWeekNumOfYear INT
	, FiscalYear INT
	, FullDate	VARCHAR(10)	
	, Period	INT
	, UNIQUE CLUSTERED (FullDate, FiscalYear, Period))
INSERT INTO @Calendar
SELECT DISTINCT FiscalWeekNumOfYear
	, FiscalYear
	, CONVERT(VARCHAR(10), FullDate, 120) 
	, Period	
FROM [HQ-FinSQL-01].Staging.dbo.vwCalendarShort

DECLARE @Result TABLE (Account VARCHAR(22), Restaurant INT, DateOfBusiness DATETIME, WeekInfo VARCHAR(12), Amount MONEY, UNIQUE CLUSTERED (Restaurant, DateOfBusiness))
INSERT INTO @Result
SELECT 'RegisteredRoyaltySales' AS Account
, ck.FKStoreID AS Restaurant
, ck.DateOfBusiness AS DateOfBusiness
,'Week ' + CONVERT(VARCHAR(2), c.FiscalWeekNumOfYear) + 
		' FY' +  SUBSTRING(CONVERT(VARCHAR(4), c.FiscalYear), 3, 2) AS WeekInfo
,SUM(CASE 
	WHEN m.ProfileAddedDateTime &lt;&gt; 0 
		AND m.ProfileAddedDateTime IS NOT NULL 
	THEN ck.CheckTotal
	ELSE 0
	END) AS Amount
FROM dbo.HstvbofqCheck ck (NOLOCK)
INNER JOIN @Hierarchy h
ON h.StoreID = ck.fkStoreID
LEFT OUTER JOIN dbo.HstvbofqAssignment a (NOLOCK)
	ON ck.FKStoreID = a.FKStoreID
	AND ck.DateOfBusiness = a.DateOfBusiness
	AND ck.FKHstvbofqAssignmentID =  a.HstvbofqAssignmentID 
LEFT OUTER JOIN dbo.vbofqMemberAccount m (NOLOCK)
ON a.FKvbofqMemberAccountID = m.vbofqMemberAccountID
JOIN @Calendar c 
ON ck.DateOfBusiness = c.FullDate
WHERE ck.DateOfBusiness &gt;= h.OpenDate	-- Restaurant is open
AND ck.DateOfBusiness IN 
	(SELECT FullDate
	 FROM @Calendar
	 WHERE Period IN 
			(SELECT Period
			FROM @Calendar
			WHERE FullDate = @Yesterday)
	 AND FiscalYear IN 
		(SELECT FiscalYear
		FROM @Calendar
		WHERE FullDate = @Yesterday)
	 AND FullDate &lt; @Today)
AND a.[Status] = 4 
AND a.FKStoreID &lt;&gt; 0
GROUP BY ck.FKStoreID
, ck.DateOfBusiness
, a.[Status]
, c.FiscalWeekNumOfYear
, c.FiscalYear
ORDER BY ck.FKStoreID
, ck.DateOfBusiness

--Get date info on when restaurants were closed during timeframe
DECLARE @ClosedDates TABLE (StoreID INT, FirstDateClosed DATETIME, LastDateClosed DATETIME, UNIQUE CLUSTERED (StoreID, FirstDateClosed,LastDateClosed))
INSERT INTO @ClosedDates
SELECT DISTINCT [StoreID]
      ,[FirstDateClosed]
      ,[LastDateClosed]
  FROM [HQ-FinSQL-01].[Interfaces].[Aloha].[ClosedRestaurants]
  order by StoreID, FirstDateClosed desc

-- Remove records for DateOfBusiness where restaurant was closed
DELETE r
FROM @Result r
JOIN @ClosedDates c
ON r.Restaurant = c.StoreID
WHERE r.DateOfBusiness &gt;= c.FirstDateClosed
  AND r.DateOfBusiness &lt;= c.LastDateClosed

select * from @Result
order by Restaurant, DateOfBusiness</property>
