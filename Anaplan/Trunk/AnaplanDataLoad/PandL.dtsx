      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{BB7A5518-49D7-4918-BA38-AF71FB377BFB}"
          SQLTask:SqlStatementSource="WITH MaxDateInWeek AS &#xA;       (SELECT cs.FiscalYear, cs.Period, cs.WeekNumInPeriod, MAX(cs.FullDate) AS FullDate&#xA;          FROM Staging.dbo.vwCalendarShort cs&#xA;       GROUP BY cs.FiscalYear, cs.Period, cs.WeekNumInPeriod)&#xA;INSERT INTO dbo.PAndL&#xA;SELECT Account&#xA;                     ,Restaurant&#xA;                     ,x.FiscalYear&#xA;                     ,x.Period&#xA;                     ,x.SubPeriod&#xA;                     ,CONVERT(INT, SUBSTRING(WeekNum, 5, 1)) WeekNum&#xA;                     ,'' WeekInfo&#xA;                     ,SUM(Amount) Amount&#xA;FROM&#xA;         (SELECT&#xA;               WeekNum&#xA;               ,Account&#xA;               ,Restaurant&#xA;               ,FiscalYear&#xA;               ,Period&#xA;               ,SubPeriod&#xA;               ,Amount&#xA;          FROM dbo.PandLDetail&#xA;          UNPIVOT (Amount&#xA;                    FOR WeekNum IN (Week1&#xA;                                   ,Week2&#xA;                                   ,Week3&#xA;                                   ,Week4&#xA;                                   ,Week5)) Amount) x&#xA;INNER JOIN MaxDateInWeek cs&#xA;       ON cs.FiscalYear = x.FiscalYear&#xA;       AND cs.Period = x.Period&#xA;       AND cs.WeekNumInPeriod = CAST(SUBSTRING(WeekNum, 5, 1) AS INT)&#xA;GROUP BY&#xA;     Account&#xA;     ,Restaurant&#xA;     ,x.FiscalYear&#xA;     ,x.Period&#xA;     ,x.WeekNum&#xA;     ,x.SubPeriod&#xA;ORDER BY&#xA;     Restaurant&#xA;     ,WeekInfo&#xA;     ,Account&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
