        <pipeline
          version="1">
          <components>
            <component
              refId="Package\Populate recent P and L data from NBO\Get NBO Data"
              componentClassID="{165A526D-D5DE-47FF-96A6-F8274C19826B}"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Get NBO Data"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
	s.CategoryID AS            [Account],
	CONVERT(INT, s.StoreId) AS [Restaurant],
	s.FiscalYear,
	s.ReportingPeriod# AS      [Period],
	0 AS                       [SubPeriod],
	'' AS                      [WeekInfo],
	CASE WHEN ISNULL(w.[Week], 0) &gt;= 1
          THEN 0
          ELSE s.AmountWeek1
     END AS           [Week1],
	CASE WHEN ISNULL(w.[Week], 0) &gt;= 2
          THEN 0
          ELSE s.AmountWeek2
     END AS           [Week2],
	CASE WHEN ISNULL(w.[Week], 0) &gt;= 3
          THEN 0
          ELSE s.AmountWeek3
     END AS           [Week3],
	CASE WHEN ISNULL(w.[Week], 0) &gt;= 4
          THEN 0
          ELSE s.AmountWeek4
     END AS           [Week4],
	CASE WHEN ISNULL(w.[Week], 0) = 5
          THEN 0
          ELSE s.AmountWeek5
     END AS           [Week5]
FROM       DataMartStaging.dbo.vwPandLCat AS s
INNER JOIN dbo.ST_CalendarSubperiods AS v
		 ON s.FiscalYear = v.[Year]
		    AND s.ReportingPeriod# = v.Period
LEFT OUTER JOIN
(
	SELECT
		p.ConfigurationValue AS [StoreID],
          c.[Year],
		c.Period,
		c.[Week]
	FROM  DataMartStaging.config.ProcessConfiguration AS p
	INNER JOIN dbo.ST_CalendarSubperiods AS c
		 ON p.ObsoleteDate BETWEEN c.StartDate AND c.EndDate
	WHERE p.ConfigurationName = 'Cubes'
		 AND p.ConfigurationCode = 'Exclusions'
) AS w
		 ON CONVERT(INT, s.StoreId) = w.StoreID
           AND s.FiscalYear = w.[Year]
           AND s.ReportingPeriod# = w.Period
LEFT OUTER JOIN DataMartStaging.config.ProcessConfiguration AS ex
		 ON ex.ConfigurationName = 'Cubes'
		    AND ex.ConfigurationCode = 'Exclusions'
		    AND CONVERT(INT, s.StoreId) = ex.ConfigurationValue
		    AND v.EndDate BETWEEN ex.EffectiveDate AND ex.ObsoleteDate
WHERE v.BusinessCalendarSubperiodNumber &gt;= ?
	 AND ex.ConfigurationValue IS NULL
ORDER BY
	s.FiscalYear,
	s.ReportingPeriod#,
	CONVERT(INT, s.StoreId);</property>
