                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
     s.CategoryId Account,
     CONVERT(INT, s.StoreId) AS 'Restaurant',
     s.FiscalYear,
     s.ReportingPeriod# AS 'Period',
     0 AS 'SubPeriod',
     '' AS 'WeekInfo',
     CASE WHEN ISNULL(w.WeekNumber, 6) = 1
          THEN 0
          ELSE s.AmountWeek1
     END AS 'Week1',
     CASE WHEN ISNULL(w.WeekNumber, 6) &lt;= 2
          THEN 0
          ELSE s.AmountWeek2
     END AS 'Week2',
     CASE WHEN ISNULL(w.WeekNumber, 6) &lt;= 3
          THEN 0
          ELSE s.AmountWeek3
     END AS 'Week3',
     CASE WHEN ISNULL(w.WeekNumber, 6) &lt;= 4
          THEN 0
          ELSE s.AmountWeek4
     END AS 'Week4',
     CASE WHEN ISNULL(w.WeekNumber, 6) &lt;= 5
          THEN 0
          ELSE s.AmountWeek5
     END AS 'Week5'
FROM dbo.ST_PandLCat s
INNER JOIN MLAddOn.dbo.Time_Periods_BI v
     ON  s.FiscalYear = v.FiscalYear
     AND s.ReportingPeriod# = v.Period
LEFT OUTER JOIN
(
	SELECT
		p.ConfigurationValue AS [StoreID],
          c.fiscalyear,
		c.Period,
		c.WeekNumber
	FROM  MLAddOn.dbo.ProcessConfiguration AS p
	INNER JOIN MLAddOn.dbo.Time_Periods_BI AS c
		 ON p.EffectiveDate = c.salesmixdate
	WHERE p.ConfigurationName = 'Cubes'
		 AND p.ConfigurationCode = 'Exclusions'
) AS w
		 ON CONVERT(INT, s.StoreId) = w.StoreID
           AND s.FiscalYear = w.fiscalyear
           AND s.ReportingPeriod# = w.Period
LEFT OUTER JOIN MLAddOn.dbo.ProcessConfiguration ex
     ON ex.ConfigurationName = 'Cubes'
     AND ex.ConfigurationCode = 'Exclusions'
     AND CONVERT(INT, s.StoreId) = ex.ConfigurationValue
     AND v.SalesMixDate BETWEEN ex.EffectiveDate AND ex.ObsoleteDate
WHERE v.Subperiod &gt;= ?
     AND CategoryID NOT IN (7750001 -- Ending Inventory - Balance Sheet account
                           ,7750002 -- Cost calculation - not required
                           ,7750009 -- NULL - not required
                           ,7750006)  -- NULL - not required
     AND ex.ConfigurationValue IS NULL
ORDER BY s.FiscalYear,
s.ReportingPeriod# , 
 CONVERT(INT, s.StoreId);</property>
