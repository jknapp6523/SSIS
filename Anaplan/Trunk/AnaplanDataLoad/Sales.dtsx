    <DTS:Executable
      DTS:refId="Package\Populate recent Sales data"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Populate recent Sales data"
      DTS:DTSID="{CF6F2D09-7413-48DB-8144-D4881BA03316}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Populate recent Sales data"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{AE3FA1B0-F02D-40BC-A424-D4A438C442A1}"
          SQLTask:SqlStatementSource="SET NOCOUNT ON&#xA;&#xA;DECLARE&#xA;     @StartDate DATETIME&#xA;&#x9;,@EndDate DATETIME&#xA;SET @StartDate = DATEADD(wk, -3, DATEADD(wk, DATEDIFF(wk, 0, CURRENT_TIMESTAMP), 0))&#xA;SET @EndDate = DATEADD(dd, 0, DATEADD(dd, DATEDIFF(dd, 0, CURRENT_TIMESTAMP), 0))&#xA;&#xA;-- Get Hierarchy info&#xA;DECLARE @Hierarchy TABLE (StoreID INT, Company VARCHAR(100), OpenDate DATETIME, UNIQUE CLUSTERED (StoreID, Company))&#xA;INSERT INTO @Hierarchy&#xA;SELECT Restaurant, Company, OpenDate&#xA;FROM [HQ-FinSQL-01].AnaplanScorecardBudget.dbo.Hierarchy&#xA;WHERE Company IN ('RRGB','RRBW') &#xA;&#xA;--Get date info on when restaurants were closed during timeframe&#xA;DECLARE @ClosedDates TABLE (StoreID INT, FirstDateClosed DATETIME, LastDateClosed DATETIME, UNIQUE CLUSTERED (StoreID, FirstDateClosed,LastDateClosed))&#xA;INSERT INTO @ClosedDates&#xA;SELECT DISTINCT [StoreID]&#xA;      ,[FirstDateClosed]&#xA;      ,[LastDateClosed]&#xA;  FROM [HQ-FinSQL-01].[Interfaces].[Aloha].[ClosedRestaurants]&#xA;  order by StoreID, FirstDateClosed desc&#xA; &#xA;&#xA;--Get the basic sales counts and totals &#xA;SELECT ISNULL(SUM(CASE WHEN Type = 10 THEN Amount END), 0) GuestCount&#xA;,ISNULL(SUM(CASE WHEN Type = 1 AND TypeID = 1000 THEN Amount END), 0) FoodSales&#xA;,ISNULL(SUM(CASE WHEN Type = 1 AND TypeID = 2000 THEN Amount END), 0) LiquorSales&#xA;,ISNULL(SUM(CASE WHEN Type = 1 AND TypeID = 3000 THEN Amount END), 0) WineSales&#xA;,ISNULL(SUM(CASE WHEN Type = 1 AND TypeID = 4000 THEN Amount END), 0) DraftBeerSales&#xA;,ISNULL(SUM(CASE WHEN Type = 1 AND TypeID = 5000 THEN Amount END), 0) BottledBeerSales&#xA;,ISNULL(SUM(CASE WHEN Type = 1 AND TypeID = 6000 THEN Amount END), 0) NonAlcoholicBeverages&#xA;,ISNULL(SUM(CASE WHEN Type = 33 THEN Amount END), 0) IncludedTax&#xA;,FKStoreID Restaurant&#xA;,ss.DateOfBusiness&#xA;,'Week ' + CONVERT(VARCHAR(2), c.FiscalWeekNumOfYear) + &#xA;' FY' + SUBSTRING(CONVERT(VARCHAR(4), c.FiscalYear), 3, 2) WeekInfo&#xA;INTO #Sales&#xA;FROM dbo.dpvHstSalesSummary ss&#xA;JOIN [HQ-FinSQL-01].Staging.dbo.vwCalendarShort c&#xA;ON ss.DateOfBusiness = c.FullDate&#xA;JOIN @Hierarchy h&#xA;ON h.StoreID = ss.fkStoreID&#xA;WHERE ss.DateOfBusiness &gt;= h.OpenDate &#xA;AND&#x9;  ss.DateOfBusiness &gt;= @StartDate&#xA;AND&#x9;  ss.DateOfBusiness &lt; @EndDate&#xA;GROUP BY FKStoreId&#xA;,ss.DateOfBusiness&#xA;,c.FiscalWeekNumOfYear&#xA;,c.FiscalYear&#xA;ORDER BY Restaurant&#xA;&#xA;-- Remove records for DateOfBusiness where restaurant was closed&#xA;DELETE s&#xA;FROM #Sales s&#xA;JOIN @ClosedDates c&#xA;ON s.Restaurant = c.StoreID&#xA;WHERE s.DateOfBusiness &gt;= c.FirstDateClosed&#xA;  AND s.DateOfBusiness &lt;= c.LastDateClosed&#xA;&#xA;&#xA;INSERT INTO [HQ-FinSQL-01].AnaplanScorecardBudget.dbo.Sales&#xA;SELECT * &#xA;FROM (SELECT Account&#xA;&#x9;&#x9;,Restaurant&#xA;&#x9;&#x9;,DateOfBusiness&#xA;&#x9;&#x9;,WeekInfo&#xA;&#x9;&#x9;,Amount&#xA;&#x9;&#x9;,CASE WHEN Account = 'GuestCount' THEN 1&#xA;&#x9;&#x9;&#x9;WHEN Account = 'FoodSales' THEN 2&#xA;&#x9;&#x9;&#x9;WHEN Account = 'LiquorSales' THEN 3&#xA;&#x9;&#x9;&#x9;WHEN Account = 'WineSales' THEN 4&#xA;&#x9;&#x9;&#x9;WHEN Account = 'DraftBeerSales' THEN 5&#xA;&#x9;&#x9;&#x9;WHEN Account = 'BottledBeerSales' THEN 6&#xA;&#x9;&#x9;&#x9;WHEN Account = 'NonAlcoholicBeverages' THEN 7&#xA;&#x9;&#x9;&#x9;WHEN Account = 'IncludedTax' THEN 8&#xA;&#x9;&#x9;&#x9;END SortOrder&#xA;&#x9;&#x9;FROM #Sales&#xA;&#x9;&#x9;UNPIVOT (Amount &#xA;&#x9;&#x9;FOR Account IN (GuestCount  &#xA;&#x9;&#x9;&#x9;,FoodSales&#xA;&#x9;&#x9;&#x9;,LiquorSales&#xA;&#x9;&#x9;&#x9;,WineSales&#xA;&#x9;&#x9;&#x9;,DraftBeerSales&#xA;&#x9;&#x9;&#x9;,BottledBeerSales&#xA;&#x9;&#x9;&#x9;,NonAlcoholicBeverages&#xA;&#x9;&#x9;&#x9;,IncludedTax)) Amount) x&#xA;ORDER BY Restaurant&#xA;,DateOfBusiness&#xA;,SortOrder&#xA;,Account" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
