                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @StartDate DATETIME
,@EndDate DATETIME

SET @StartDate = DATEADD(wk, -3, DATEADD(wk, DATEDIFF(wk, 0, CURRENT_TIMESTAMP), 0))
SET @EndDate = DATEADD(dd, 0, DATEADD(dd, DATEDIFF(dd, 0, CURRENT_TIMESTAMP), 0))

SELECT Account
,Restaurant
,DateOfBusiness
,WeekInfo
,Amount
FROM dbo.Sales
WHERE Amount &lt;&gt; 0
AND DateOfBusiness &gt;= @StartDate
AND DateOfBusiness &lt; @EndDate
AND Restaurant IN (SELECT DISTINCT Restaurant
FROM dbo.Hierarchy)
ORDER BY Restaurant
,DateOfBusiness
,SortOrder
,Account</property>
