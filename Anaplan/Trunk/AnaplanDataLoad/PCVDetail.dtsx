    <DTS:Executable
      DTS:refId="Package\Populate recent PCVDetail data"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Populate recent PCVDetail data"
      DTS:DTSID="{E876028F-8D87-4746-A43B-1910E7078D10}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Populate recent PCVDetail data"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{AE3FA1B0-F02D-40BC-A424-D4A438C442A1}"
          SQLTask:SqlStatementSource="SET NOCOUNT ON&#xA;&#xA;DECLARE @StartDate DATETIME, @EndDate DATETIME&#xA;SET @StartDate = DATEADD(wk, -3, DATEADD(wk, DATEDIFF(wk, 0, CURRENT_TIMESTAMP), 0)) &#xA;SET @EndDate = DATEADD(dd, 0, DATEADD(dd, DATEDIFF(dd, 0, CURRENT_TIMESTAMP), 0))&#xA;&#xA;-- No franchise restaurants&#xA;DECLARE @Hierarchy TABLE (StoreID INT, Company VARCHAR(100), OpenDate DATETIME, UNIQUE CLUSTERED (StoreID, Company))&#xA;INSERT INTO @Hierarchy&#xA;SELECT Restaurant, Company, OpenDate&#xA;FROM [HQ-FinSQL-01].AnaplanScorecardBudget.dbo.Hierarchy&#xA;WHERE Company IN ('RRGB','RRBW') &#xA;&#xA;--Get date info on when restaurants were closed during timeframe&#xA;DECLARE @ClosedDates TABLE (StoreID INT, FirstDateClosed DATETIME, LastDateClosed DATETIME, UNIQUE CLUSTERED (StoreID, FirstDateClosed,LastDateClosed))&#xA;INSERT INTO @ClosedDates&#xA;SELECT DISTINCT [StoreID]&#xA;      ,[FirstDateClosed]&#xA;      ,[LastDateClosed]&#xA;  FROM [HQ-FinSQL-01].[Interfaces].[Aloha].[ClosedRestaurants]&#xA;  order by StoreID, FirstDateClosed desc&#xA;&#xA;--Get Promo data&#xA;SELECT p.DateOfBusiness&#xA;, p.FKStoreId Restaurant&#xA;, pr.Name&#xA;, CASE WHEN xref.CategoryId = 1 THEN SUM(p.Amount) END RestPromo&#xA;, CASE WHEN xref.CategoryId = 1 THEN COUNT(xref.CategoryID) END RestPromoCnt&#xA;, CASE WHEN xref.CategoryId = 2 THEN SUM(p.Amount) END CorpPromo&#xA;, CASE WHEN xref.CategoryId = 2 THEN COUNT(xref.CategoryID) END CorpPromoCnt&#xA;, CASE WHEN xref.CategoryId = 3 THEN SUM(p.Amount) END TMMeal&#xA;, CASE WHEN xref.CategoryId = 3 THEN COUNT(xref.CategoryID) END TMMealCnt&#xA;, CASE WHEN xref.CategoryId = 4 THEN SUM(p.Amount) END MgrMeal&#xA;, CASE WHEN xref.CategoryId = 4 THEN COUNT(xref.CategoryID) END MgrMealCnt&#xA;, 'Week ' + CONVERT(VARCHAR(2), c.FiscalWeekNumOfYear) + &#xA;&#x9;&#x9;' FY' +  SUBSTRING(CONVERT(VARCHAR(4), c.FiscalYear), 3, 2) WeekInfo&#xA;INTO #PromoDetail&#xA;FROM dbo.dpvHstPromotion p&#xA;JOIN dbo.Promotion pr&#xA;&#x9;ON p.FKPromotionID = pr.PromotionID&#xA;JOIN [HQ-FinSQL-01].Discounts.dbo.XrefPCV xRef&#xA;&#x9;ON p.FKPromotionId = xRef.Id&#xA;JOIN [HQ-FinSQL-01].Staging.dbo.vwCalendarShort c&#xA;&#x9;ON p.DateOfBusiness = c.FullDate&#xA;JOIN [HQ-FinSQL-01].DW.dbo.StoreList s&#xA;&#x9;ON p.FKStoreID = s.StoreNum&#xA;JOIN @Hierarchy h&#xA;ON h.StoreID = s.StoreNum&#xA;WHERE p.DateOfBusiness &gt;= @StartDate&#xA;AND p.DateOfBusiness &lt; @EndDate&#xA;AND p.DateOfBusiness &gt;= h.OpenDate&#xA;AND xRef.TypeId = 'P'&#xA;AND xRef.CategoryId &lt;&gt; 0&#xA;AND s.Category IN ('O', 'N', 'F')&#xA;GROUP BY p.DateOfBusiness&#xA;&#x9;&#x9;&#x9;&#x9;, p.FKStoreId&#xA;&#x9;&#x9;&#x9;&#x9;, pr.Name&#xA;&#x9;&#x9;&#x9;&#x9;, c.FiscalWeekNumOfYear&#xA;&#x9;&#x9;&#x9;&#x9;, c.FiscalYear&#xA;&#x9;&#x9;&#x9;&#x9;, xref.CategoryId&#xA;ORDER BY p.FKStoreID&#xA;&#x9;&#x9;&#x9;&#x9;, p.DateOfBusiness&#xA;&#x9;&#x9;&#x9;&#x9;, pr.Name&#xA;&#xA;-- Remove Promo data from days store was closed &#xA;DELETE d&#xA;FROM #PromoDetail d&#xA;JOIN @ClosedDates c&#xA;ON d.Restaurant = c.StoreID&#xA;WHERE d.DateOfBusiness &gt;= c.FirstDateClosed&#xA;  AND d.DateOfBusiness &lt;= c.LastDateClosed&#xA;&#xA;&#xA;&#xA;--Get Comp data&#xA;SELECT hc.FKStoreId Restaurant&#xA;, hc.DateOfBusiness&#xA;, comp.Name&#xA;, CASE WHEN xref.CategoryId = 5 THEN SUM(hc.Amount) END GstComplaint&#xA;, CASE WHEN xref.CategoryId = 5 THEN COUNT(xref.CategoryId) END GstComplaintCnt&#xA;, CASE WHEN xref.CategoryId = 6 THEN SUM(hc.Amount) END Walkout&#xA;, CASE WHEN xref.CategoryId = 6 THEN COUNT(xref.CategoryId) END WalkoutCnt&#xA;, CASE WHEN xref.CategoryId = 7 THEN SUM(hc.Amount) END CompGC&#xA;, CASE WHEN xref.CategoryId = 7 THEN COUNT(xref.CategoryId) END CompGCCnt&#xA;, 'Week ' + CONVERT(VARCHAR(2), c.FiscalWeekNumOfYear) + ' FY' &#xA;&#x9;&#x9;+  SUBSTRING(CONVERT(VARCHAR(4), c.FiscalYear), 3, 2) WeekInfo&#xA;INTO #CompDetail&#xA;FROM dbo.dpvHstComp hc&#xA;JOIN dbo.Comp comp&#xA;ON hc.FKCompID = comp.CompID&#xA;JOIN [HQ-FinSQL-01].Discounts.dbo.XrefPCV xRef&#xA;ON hc.FKCompId = xRef.Id&#xA;JOIN [HQ-FinSQL-01].Staging.dbo.vwCalendarShort c&#xA;ON hc.DateOfBusiness = c.FullDate&#xA;JOIN [HQ-FinSQL-01].DW.dbo.StoreList s&#xA;ON hc.FKStoreID = s.StoreNum&#xA;JOIN @Hierarchy h&#xA;ON h.StoreID = s.StoreNum&#xA;WHERE hc.DateOfBusiness &gt;= @StartDate&#xA;AND hc.DateOfBusiness &gt;= @StartDate&#xA;AND hc.DateOfBusiness &lt; @EndDate&#xA;AND xRef.TypeId = 'C'&#xA;AND xRef.CategoryId &lt;&gt; 0&#xA;AND s.Category IN ('O', 'N', 'F')&#xA;GROUP BY hc.FKStoreId&#xA;, hc.DateOfBusiness&#xA;, xref.CategoryId&#xA;, c.FiscalWeekNumOfYear&#xA;, c.FiscalYear&#xA;, comp.Name&#xA;ORDER BY hc.FKStoreID&#xA;, hc.DateOfBusiness&#xA;, comp.Name&#xA;&#xA;-- Remove Comp data from days store was closed &#xA;DELETE d&#xA;FROM #CompDetail d&#xA;JOIN @ClosedDates c&#xA;ON d.Restaurant = c.StoreID&#xA;WHERE d.DateOfBusiness &gt;= c.FirstDateClosed&#xA;  AND d.DateOfBusiness &lt;= c.LastDateClosed&#xA;&#xA;&#xA;--Get Void data&#xA;SELECT v.FKStoreId Restaurant&#xA;, v.DateOfBusiness&#xA;, i.ShortName Name&#xA;, CASE WHEN xref.CategoryId = 8 THEN SUM(v.Price) END GstRemake&#xA;, CASE WHEN xref.CategoryId = 8 THEN COUNT(xref.CategoryId) END GstRemakeCnt&#xA;, CASE WHEN xref.CategoryId = 9 THEN SUM(v.Price) END ORNotMade&#xA;, CASE WHEN xref.CategoryId = 9 THEN COUNT(xref.CategoryId) END ORNotMadeCnt&#xA;, CASE WHEN xref.CategoryId = 10 THEN SUM(v.Price) END ProductDemo&#xA;, CASE WHEN xref.CategoryId = 10 THEN COUNT(xref.CategoryId) END ProductDemoCnt&#xA;, CASE WHEN xref.CategoryId = 11 THEN SUM(v.Price) END HOHFreelMeal&#xA;, CASE WHEN xref.CategoryId = 11 THEN COUNT(xref.CategoryId) END HOHFreelMealCnt&#xA;, CASE WHEN xref.CategoryId = 12 THEN SUM(v.Price) END CLCFreeMeal&#xA;, CASE WHEN xref.CategoryId = 12 THEN COUNT(xref.CategoryId) END CLCFreeMealCnt&#xA;, '            ' WeekInfo&#xA;INTO #VoidDetail&#xA;FROM dbo.dpvHstGndVoid v&#xA;JOIN dbo.Item i&#xA;ON v.FKItemID = i.ItemId&#xA;JOIN [HQ-FinSQL-01].Discounts.dbo.XrefPCV xRef&#xA;ON v.FKReasonId = xRef.Id&#xA;JOIN @Hierarchy h&#xA;ON h.StoreID = v.FKStoreId&#xA;WHERE v.DateOfBusiness &gt;= @StartDate&#xA;AND v.DateOfBusiness &gt;= @StartDate&#xA;AND v.DateOfBusiness &lt; @EndDate&#xA;AND xRef.TypeId = 'V'&#xA;AND xRef.CategoryId &lt;&gt; 0&#xA;GROUP BY v.FKStoreId&#xA;, v.DateOfBusiness&#xA;, i.ShortName&#xA;, xRef.CategoryId&#xA;ORDER BY v.FKStoreId&#xA;, v.DateOfBusiness&#xA;, i.ShortName&#xA;&#xA;DELETE FROM #VoidDetail&#xA;WHERE Restaurant NOT IN &#xA;&#x9;(SELECT DISTINCT StoreID&#xA;&#x9;FROM [HQ-FinSQL-01].DW.dbo.StoreList &#xA;&#x9;WHERE Category IN ('O', 'N', 'F')) &#xA;&#xA;&#xA;&#xA;-- Remove Void data from days store was closed &#xA;DELETE d&#xA;FROM #VoidDetail d&#xA;JOIN @ClosedDates c&#xA;ON d.Restaurant = c.StoreID&#xA;WHERE d.DateOfBusiness &gt;= c.FirstDateClosed&#xA;  AND d.DateOfBusiness &lt;= c.LastDateClosed&#xA;&#xA;&#xA;UPDATE #VoidDetail&#xA;SET WeekInfo = 'Week ' + CONVERT(VARCHAR(2), c.FiscalWeekNumOfYear) + &#xA;&#x9;&#x9;&#x9;&#x9;' FY' +  SUBSTRING(CONVERT(VARCHAR(4), c.FiscalYear), 3, 2)&#xA;FROM [HQ-FinSQL-01].[Staging].[dbo].[vwCalendarShort] c&#xA;WHERE #VoidDetail.DateOfBusiness = c.FullDate&#xA;&#xA;--UnPivot the Count and Amount columns&#xA;SELECT Account&#xA;, Restaurant&#xA;, DateOfBusiness&#xA;, WeekInfo&#xA;, Name&#xA;, Cnt&#xA;, SUM(Amount) Amount&#xA;INTO #PCVDetail&#xA;FROM (SELECT 'Promotions - ' + Account Account&#xA;&#x9;, Restaurant&#xA;&#x9;, DateOfBusiness&#xA;&#x9;, WeekInfo&#xA;&#x9;, Name&#xA;&#x9;, Cnt&#xA;&#x9;, Amount&#xA;&#x9;FROM #PromoDetail&#xA;&#x9;UNPIVOT (Cnt&#xA;&#x9;&#x9;FOR Promot IN (RestPromoCnt&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;, CorpPromoCnt&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;, TMMealCnt&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;, MgrMealCnt)) Cnt&#xA;&#x9;UNPIVOT (Amount&#xA;&#x9;&#x9;FOR Account IN (RestPromo&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;, CorpPromo&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;, TMMeal&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;, MgrMeal)) Amount) p&#xA;GROUP BY Restaurant&#xA;, DateOfBusiness&#xA;, WeekInfo&#xA;, Name&#xA;, Cnt&#xA;, Account&#xA;UNION ALL&#xA;SELECT Account&#xA;, Restaurant&#xA;, DateOfBusiness&#xA;, WeekInfo&#xA;, Name&#xA;, Cnt&#xA;, SUM(Amount) Amount&#xA;FROM (SELECT 'Comps - ' + Account Account&#xA;&#x9;&#x9;, Restaurant&#xA;&#x9;&#x9;, DateOfBusiness&#xA;&#x9;&#x9;, WeekInfo&#xA;&#x9;&#x9;, Name&#xA;&#x9;&#x9;, Cnt&#xA;&#x9;&#x9;, Amount&#xA;&#x9;&#x9;FROM #CompDetail&#xA;&#x9;&#x9;UNPIVOT (Cnt&#xA;&#x9;&#x9;&#x9;FOR Comp IN (GstComplaintCnt&#xA;&#x9;&#x9;&#x9;, WalkoutCnt&#xA;&#x9;&#x9;&#x9;, CompGCCnt)) Cnt&#xA;&#x9;&#x9;UNPIVOT (Amount&#xA;&#x9;&#x9;&#x9;FOR Account IN (GstComplaint&#xA;&#x9;&#x9;&#x9;, Walkout&#xA;&#x9;&#x9;&#x9;, CompGC)) Amount) c&#xA;GROUP BY Restaurant&#xA;, DateOfBusiness&#xA;, WeekInfo&#xA;, Name&#xA;, Cnt&#xA;, Account&#xA;UNION ALL&#xA;SELECT Account&#xA;, Restaurant&#xA;, DateOfBusiness&#xA;, WeekInfo&#xA;, Name&#xA;, Cnt&#xA;, SUM(Amount) Amount&#xA;FROM (SELECT 'Voids - ' + Account Account&#xA;&#x9;, Restaurant&#xA;&#x9;, DateOfBusiness&#xA;&#x9;, WeekInfo&#xA;&#x9;, Name&#xA;&#x9;, Cnt&#xA;&#x9;, Amount&#xA;&#x9;FROM #VoidDetail&#xA;&#x9;UNPIVOT (Cnt&#xA;&#x9;FOR Void IN (GstRemakeCnt&#xA;&#x9;&#x9;&#x9;&#x9;, ORNotMadeCnt&#xA;&#x9;&#x9;&#x9;&#x9;, ProductDemoCnt&#xA;&#x9;&#x9;&#x9;&#x9;, HOHFreelMealCnt&#xA;&#x9;&#x9;&#x9;&#x9;, CLCFreeMealCnt)) Cnt&#xA;&#x9;UNPIVOT (Amount&#xA;&#x9;FOR Account IN (GstRemake&#xA;&#x9;&#x9;&#x9;&#x9;, ORNotMade&#xA;&#x9;&#x9;&#x9;&#x9;, ProductDemo&#xA;&#x9;&#x9;&#x9;&#x9;, HOHFreelMeal&#xA;&#x9;&#x9;&#x9;&#x9;, CLCFreeMeal)) Amount) v&#xA;GROUP BY Restaurant&#xA;, DateOfBusiness&#xA;, WeekInfo&#xA;, Name&#xA;, Cnt&#xA;, Account&#xA;ORDER BY Restaurant&#xA;, DateOfBusiness&#xA;, Name&#xA;&#xA;--Delete unnecessary rows&#xA;DELETE FROM #PCVDetail &#xA;WHERE Amount = 0&#xA;&#xA;&#xA;-- Remove #PCVDetail data from days store was closed &#xA;DELETE d&#xA;FROM #PCVDetail d&#xA;JOIN @ClosedDates c&#xA;ON d.Restaurant = c.StoreID&#xA;WHERE d.DateOfBusiness &gt;= c.FirstDateClosed&#xA;  AND d.DateOfBusiness &lt;= c.LastDateClosed&#xA;&#xA;----Insert into the master table - add sortability with the case statement&#xA;INSERT INTO [HQ-FinSQL-01].AnaplanScorecardBudget.dbo.PCVDetail&#xA;SELECT Account&#xA;, Restaurant &#xA;, DateOfBusiness&#xA;, WeekInfo&#xA;, Name&#xA;, Cnt&#xA;, Amount&#xA;, CASE WHEN Account = 'Promotions - RestPromo' THEN 1&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Promotions - CorpPromo' THEN 2&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Promotions - TMMeal' THEN 3&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Promotions - MgrMeal' THEN 4&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Comps - GstComplaint' THEN 5&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Comps - Walkout' THEN 6&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Comps - CompGC' THEN 7&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Voids - GstRemake' THEN 8&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Voids - ORNotMade' THEN 9&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Voids - ProductDemo' THEN 10&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Voids - HOHFreelMeal' THEN 11&#xA;&#x9;&#x9;&#x9;WHEN Account = 'Voids - CLCFreeMeal' THEN 12&#xA;&#x9;END SortOrder&#xA;FROM #PCVDetail&#xA;ORDER BY Restaurant&#xA;, DateOfBusiness&#xA;, Name" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
