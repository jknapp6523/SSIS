    <DTS:Executable
      DTS:refId="Package\Populate recent OrderRate data"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Populate recent OrderRate data"
      DTS:DTSID="{307EEB31-5743-4227-8ADA-873EFF1F9498}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Populate recent OrderRate data"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{AE3FA1B0-F02D-40BC-A424-D4A438C442A1}"
          SQLTask:SqlStatementSource="DECLARE @StartDate DATETIME, @EndDate DATETIME&#xA;SET @StartDate = DATEADD(wk, -3, DATEADD(wk, DATEDIFF(wk, 0, CURRENT_TIMESTAMP), 0)) &#xA;SET @EndDate = DATEADD(dd, 0, DATEADD(dd, DATEDIFF(dd, 0, CURRENT_TIMESTAMP), 0))&#xA;&#xA;DECLARE @Hierarchy TABLE (StoreID INT, Company VARCHAR(100), OpenDate DATETIME, UNIQUE CLUSTERED (StoreID, Company))&#xA;INSERT INTO @Hierarchy&#xA;SELECT Restaurant, Company, OpenDate&#xA;FROM [HQ-FinSQL-01].AnaplanScorecardBudget.dbo.Hierarchy&#xA;WHERE Company IN ('RRGB','RRBW') &#xA;&#xA;--Get date info on when restaurants were closed during timeframe&#xA;DECLARE @ClosedDates TABLE (StoreID INT, FirstDateClosed DATETIME, LastDateClosed DATETIME, UNIQUE CLUSTERED (StoreID, FirstDateClosed,LastDateClosed))&#xA;INSERT INTO @ClosedDates&#xA;SELECT DISTINCT [StoreID]&#xA;      ,[FirstDateClosed]&#xA;      ,[LastDateClosed]&#xA;  FROM [HQ-FinSQL-01].[Interfaces].[Aloha].[ClosedRestaurants]&#xA;  order by StoreID, FirstDateClosed desc&#xA;&#xA;SELECT FKStoreId, DateOfBusiness, FKItemId, COUNT(FKItemId) AS Amount INTO #Temp &#xA;FROM dbo.dpvHstGndItem &#xA;WHERE DateOfBusiness &gt;= @StartDate AND DateOfBusiness &lt; @EndDate&#xA;     AND DiscPric &gt; 0&#xA;     AND (ParentID = 0&#xA;&#x9; OR FKItemId IN  (5751,5796))&#xA;GROUP BY FKStoreId, DateOfBusiness, FKItemId&#xA;&#xA;&#xA;--Get Appetizer, Dessert and Beverage counts&#xA;--Find all dbo.dpvHstGndItem ParentID's and their descriptions&#xA;SELECT CASE &#xA;&#x9;&#x9;&#x9;WHEN REPLACE(ci.CategoryItemId, '*', '') = 672 --Reporting Appts - Appetizers Sold&#xA;&#x9;&#x9;&#x9;&#x9;THEN 'Appetizers Sold'&#xA;&#x9;&#x9;&#x9;WHEN REPLACE(ci.CategoryItemId, '*', '') = 671 --Reporting Desserts - Desserts Sold&#xA;&#x9;&#x9;&#x9;&#x9;THEN 'Desserts Sold'&#xA;&#x9;&#x9;&#x9;WHEN REPLACE(ci.CategoryItemId, '*', '') = 670 --Report Bev Ordr Rate - Beverages Sold&#xA;&#x9;&#x9;&#x9;&#x9;THEN 'Beverages Sold'&#xA;&#x9;&#x9;END Account&#xA;, gi.FKStoreId Restaurant&#xA;, gi.DateOfBusiness&#xA;, '' WeekInfo --,'Week ' + CONVERT(VARCHAR(2), c.FiscalWeekNumOfYear) + ' FY' +  SUBSTRING(CONVERT(VARCHAR(4), c.FiscalYear), 3, 2) WeekInfo&#xA;, SUM(gi.Amount) AS Amount&#xA;, 0 SortOrder --Not used in this metric&#xA;INTO #OrderRate&#xA;FROM #Temp (NOLOCK) gi &#xA;INNER JOIN @Hierarchy h&#xA;ON gi.FKStoreID = h.StoreID&#xA;LEFT JOIN dbo.Item i &#xA;ON gi.FKItemid = i.ItemID &#xA;LEFT JOIN dbo.CategoryItem (NOLOCK) ci&#xA;ON i.ItemId = ci.FKItemId&#xA;WHERE gi.DateOfBusiness &gt;= h.OpenDate&#xA;AND ci.CategoryItemId IN (672, 671, 670) --(906, 919, 802, 670, 671, 672)&#xA;/*&#xA;&#x9;670&#x9;Report Bev Ordr Rate - Beverages Sold&#xA;&#x9;671&#x9;Reporting Desserts - Desserts Sold&#xA;&#x9;672&#x9;Reporting Appts - Appetizers Sold&#xA;&#x9;802&#x9;Bevs-For Bev PPA&#xA;&#x9;906&#x9;Appetizers - Appetizers Desserts Sold&#xA;&#x9;919&#x9;Dessert Category&#xA;*/&#xA;GROUP BY ci.CategoryItemId&#xA;,gi.FKStoreID&#xA;,gi.DateOfBusiness&#xA;&#xA;-- Remove data from when restaurant was closed&#xA;DELETE r&#xA;FROM #OrderRate r&#xA;JOIN @ClosedDates c&#xA;ON r.Restaurant = c.StoreID&#xA;WHERE r.DateOfBusiness &gt;= c.FirstDateClosed&#xA;AND r.DateofBusiness &lt;= c.LastDateClosed&#xA;&#xA;&#xA;INSERT INTO [HQ-FinSQL-01].AnaplanScorecardBudget.dbo.OrderRate&#xA;SELECT * FROM #OrderRate" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
