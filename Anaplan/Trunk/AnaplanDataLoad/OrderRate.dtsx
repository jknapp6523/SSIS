      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{BB7A5518-49D7-4918-BA38-AF71FB377BFB}"
          SQLTask:SqlStatementSource="DECLARE @StartDate DATETIME&#xA;&#x9;&#x9;&#x9; ,@EndDate DATETIME&#xA;&#x9;SET @StartDate = DATEADD(wk, -3, DATEADD(wk, DATEDIFF(wk, 0, CURRENT_TIMESTAMP), 0)) &#xA;&#x9;SET @EndDate = DATEADD(dd, 0, DATEADD(dd, DATEDIFF(dd, 0, CURRENT_TIMESTAMP), 0))&#xA;&#xA;UPDATE dbo.OrderRate&#xA;&#x9;SET WeekInfo = 'Week ' + &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CONVERT(VARCHAR(2), FiscalWeekNumOfYear) + ' FY' +  &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;SUBSTRING(CONVERT(VARCHAR(4), FiscalYear), 3, 2) &#xA;&#x9;FROM Staging.dbo.vwCalendarShort&#xA;&#x9;WHERE DateOfBusiness = FullDate&#xA;&#x9;&#x9;AND DateOfBusiness &gt;= @StartDate&#xA;&#x9;AND DateOfBusiness &lt; @EndDate" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
