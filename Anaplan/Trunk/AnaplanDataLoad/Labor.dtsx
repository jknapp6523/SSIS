    <DTS:Executable
      DTS:refId="Package\Populate recent Labor data"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Populate recent Labor data"
      DTS:DTSID="{B51623FA-7A6B-4BDC-A6CB-287D04815F9A}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Populate recent Labor data"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{BB7A5518-49D7-4918-BA38-AF71FB377BFB}"
          SQLTask:SqlStatementSource="DECLARE @StartDate DATETIME&#xA;,@EndDate DATETIME&#xA;SET @StartDate = DATEADD(wk, -3, DATEADD(wk, DATEDIFF(wk, 0, CURRENT_TIMESTAMP), 0)) &#xA;SET @EndDate = DATEADD(dd, 0, DATEADD(dd, DATEDIFF(dd, 0, CURRENT_TIMESTAMP), 0))&#xA;&#xA;DECLARE @Hierarchy TABLE (StoreID INT, Company VARCHAR(100), OpenDate DATETIME, UNIQUE CLUSTERED (StoreID, Company))&#xA;INSERT INTO @Hierarchy&#xA;SELECT Restaurant, Company, OpenDate&#xA;FROM dbo.Hierarchy&#xA;WHERE Company IN ('RRGB','RRBW') &#xA;&#xA;--Get date info on when restaurants were closed during timeframe&#xA;DECLARE @ClosedDates TABLE (StoreID INT, FirstDateClosed DATETIME, LastDateClosed DATETIME, UNIQUE CLUSTERED (StoreID, FirstDateClosed,LastDateClosed))&#xA;INSERT INTO @ClosedDates&#xA;SELECT DISTINCT [StoreID]&#xA;      ,[FirstDateClosed]&#xA;      ,[LastDateClosed]&#xA;  FROM Interfaces.Aloha.ClosedRestaurants&#xA;  order by StoreID, FirstDateClosed desc&#xA;&#xA;&#xA;SELECT d.Location Restaurant&#xA;,d.DateOfBusiness&#xA;,o.OpenDate --Restaurant Open Date&#xA;,'Week ' + CONVERT(VARCHAR(2), FiscalWeekNumOfYear) + &#xA;&#x9;&#x9;' FY' +  SUBSTRING(CONVERT(VARCHAR(4), FiscalYear), 3, 2) WeekInfo&#xA;,d.FOHActual&#xA;,d.FOHStandard&#xA;,d.HOHActual&#xA;,d.HOHStandard&#xA;INTO #DL&#xA;FROM Labor.dbo.DailyLaborReport d&#xA;JOIN @Hierarchy h&#xA;on d.Location = h.StoreID&#xA;JOIN Staging.dbo.vwCalendarShort c&#xA;ON d.DateOfBusiness = c.FullDate&#xA;LEFT JOIN Staging.dbo.vwStoreALL o&#xA;ON d.Location = o.StoreID&#xA;WHERE d.DateOfBusiness &gt;= h.OpenDate&#x9;&#xA;AND d.DateOfBusiness &gt;= @StartDate&#xA;AND d.DateOfBusiness &lt; @EndDate&#xA;AND CONVERT(DATE, d.DateOfBusiness) &gt;= o.OpenDate&#xA;ORDER BY d.Location&#xA;,d.DateOfBusiness&#xA;&#xA;&#xA;DECLARE @Labor TABLE ([Account] [varchar](50) NULL,&#xA;&#x9;[Restaurant] [int] NULL,&#xA;&#x9;[DateOfBusiness] [datetime] NULL,&#xA;&#x9;[WeekInfo] [varchar](50) NULL,&#xA;&#x9;[Amount] [decimal](16, 2) NULL,&#xA;&#x9;UNIQUE CLUSTERED ([Restaurant], [DateOfBusiness], [Account]))&#xA;&#xA;&#xA;INSERT INTO @Labor&#xA;SELECT *&#xA;FROM (SELECT Account&#xA;&#x9;,Restaurant&#xA;&#x9;,DateOfBusiness&#xA;&#x9;,WeekInfo&#xA;&#x9;,Amount&#xA;&#x9;FROM #DL d&#xA;&#x9;UNPIVOT (Amount &#xA;&#x9;&#x9;&#x9;FOR Account IN (d.FOHActual&#xA;&#x9;&#x9;&#x9;,d.FOHStandard&#xA;&#x9;&#x9;&#x9;,d.HOHActual&#xA;&#x9;&#x9;&#x9;,d.HOHStandard)) Amount) x&#xA;ORDER BY Restaurant&#xA;,DateOfBusiness&#xA;,Account&#xA;&#xA;-- Remove data from when restaurant was closed&#xA;DELETE l&#xA;FROM @Labor l&#xA;JOIN @ClosedDates c&#xA;ON l.Restaurant = c.StoreID&#xA;WHERE l.DateOfBusiness &gt;= c.FirstDateClosed&#xA;AND l.DateofBusiness &lt;= c.LastDateClosed&#xA;&#xA;INSERT INTO dbo.Labor&#xA;SELECT * FROM @Labor&#xA;ORDER BY Restaurant&#xA;,DateOfBusiness&#xA;,Account" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
