                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT a.SubPeriod, a.SiteNumber, a.StoreID, a.RawMaterialItemCatName, a.RawMaterialItemCatID, 
	a.RawMaterialItemNumber, a.RawMaterialItemUnitNumber, a.EndingOnHandCount, 
	a.EndingItemCost, a.EndingInventory$, a.SubPeriodStartDate, 
	a.SubPeriodEndDate 
FROM dbo.ST_CostOfGoods a 
JOIN ( 
	SELECT aa.SiteNumber, MAX(aa.SubPeriod) AS MaxSubPeriod 
	FROM dbo.ST_CostOfGoods aa 
	WHERE 
		aa.EndingOnHandCount &gt; 0 AND 
		COALESCE(aa.EndingItemCost, 0) &lt;&gt; 0 
	GROUP BY aa.SiteNumber) b ON 
	a.SiteNumber = b.SiteNumber AND 
	a.SubPeriod = b.MaxSubPeriod</property>
