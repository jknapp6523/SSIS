            <component
              refId="Package\Populate Header and Detail\Existing Items"
              componentClassID="{165A526D-D5DE-47FF-96A6-F8274C19826B}"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Existing Items"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @LookbackDays INT 
DECLARE @DateStart DATETIME 

SET @LookbackDays = ? 
SET @DateStart = ? 

SELECT 
	--HEADER COLUMNS
	a.InvoiceID, 
	a.StoreID, 
	CAST('Finalized' AS VARCHAR(25)) AS InvoiceStatus, 
	CAST('Import as Pending' AS VARCHAR(25)) AS ExceptionHandling, 
	a.CustomerNumber, 
	CAST('DI' AS VARCHAR(25)) AS SupplierID, 
	DATEADD(MONTH, -5, GETDATE()) AS InvoiceDate, 
	DATEADD(MONTH, -5, GETDATE()) AS DeliveryDate, 
	CAST(0 AS DECIMAL(12, 2)) AS InvoiceTaxAmount, 
	'False' AS InvoiceAdjustment,  

	--DETAIL COLUMNS 
	ROW_NUMBER() OVER(PARTITION BY a.StoreID ORDER BY a.VendorItemCode) AS LineItemNumber, 
	a.QuantityReceived, 
	a.Price, 
	a.QuantityReceived * a.Price AS Extension, 
	a.VendorItemCode, 
	a.ItemName, 
	a.UOM_MLHQ, 
	a.UOM_NBO
FROM (
	SELECT 
		--HEADER COLUMNS 
		CAST('DI' + CONVERT(NVARCHAR(10), DATEADD(MONTH, -5, 
			GETDATE()), 112) + aa.StoreID AS VARCHAR(25)) AS InvoiceID, 
		CAST(aa.StoreID AS NVARCHAR(25)) AS StoreID, 
		CAST(aa.StoreID AS VARCHAR(25)) AS CustomerNumber, 
		 
		--DETAIL COLUMNS 
		CAST(CEILING(MAX(aa.EndingOnHandCount)) AS INT) AS QuantityReceived, 
		ROUND(CAST(MAX(aa.EndingItemCost) AS DECIMAL(12, 2)), 2) AS Price, 
		CAST(bb.ItemNumber AS VARCHAR(50)) AS VendorItemCode, 
		CAST(bb.ItemName AS VARCHAR(255)) AS ItemName, 
		CAST(MAX(aa.ConversionUnitDesc) AS VARCHAR(25)) AS UOM_MLHQ, 
		CAST(MAX(gg.ItemUnitOfMeasureName) AS NVARCHAR(50)) AS UOM_NBO	
	FROM mlhq.ST_CostOfGoods aa 
	JOIN nbo.ST_Items bb ON 
		aa.RawMaterialItemID = bb.ItemID 
	JOIN nbo.ST_ItemCategories cc ON 
		bb.ItemCategoryNumber = cc.ItemCategoryNumber 
	JOIN nbo.ST_Sites dd ON 
		aa.StoreID = RIGHT('000' + dd.StoreID, 3) 
	JOIN nbo.ST_VendorItems ee ON 
		bb.ItemNumber = ee.ItemNumber AND 
		dd.SiteNumber = ee.SiteNumber AND 
		CAST(bb.ItemNumber AS NVARCHAR(50)) = ee.VendorItemCode
	JOIN ( 
		SELECT aaa.SiteNumber, MIN(aaa.VendorNumber) AS MinDummyVendorNumber
		FROM nbo.ST_Vendors aaa 
		WHERE 
			UPPER(aaa.VendorID) = 'DUMMY VENDOR' AND 
			UPPER(aaa.VendorName) = 'DUMMY VENDOR'
		GROUP BY aaa.SiteNumber) ff ON 
		ee.SiteNumber = ff.SiteNumber AND 
		ee.VendorNumber = ff.MinDummyVendorNumber  
	LEFT JOIN nbo.ST_ItemUnitsOfMeasure gg ON 
		ee.ReceiveUnitNumber = gg.ItemUnitOfMeasureNumber
	WHERE
		EndingOnHandCount &gt; 0 AND 
		EXISTS ( 
			SELECT 1 
			FROM config.ProcessConfiguration zz 
			WHERE 
				aa.StoreID = RIGHT('000' + zz.ConfigurationValue, 3) AND 
				zz.ConfigurationName = 'Fusion' AND 
				zz.ConfigurationCode = 'Inclusion' AND 
				ABS(DATEDIFF(DAY, @DateStart, zz.EffectiveDate)) &lt;= @LookbackDays) AND 
		EXISTS ( 
			SELECT 1 
			FROM config.ProcessConfiguration yy 
			WHERE 
				cc.ItemCategoryID = yy.ConfigurationValue AND 
				yy.ConfigurationName = 'InitialPricingInvoices' AND 
				yy.ConfigurationCode = 'CategoryID') 
	GROUP BY aa.StoreID, CAST(bb.ItemNumber AS VARCHAR(50)), CAST(bb.ItemName AS VARCHAR(255)) 
	) a</property>
