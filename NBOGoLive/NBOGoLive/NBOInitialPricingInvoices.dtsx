                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @AuditKey INT 
DECLARE @InvoiceID VARCHAR(25) 

SET @AuditKey = ? 
SET @InvoiceID = ? 

SELECT '&lt;?xml version="1.0" encoding="UTF-8"?&gt;' AS XML_COL

UNION 

SELECT (
	SELECT 
		(
			SELECT InvoiceStatus AS [Status], ExceptionHandling, CustomerNumber, SupplierID, 
				InvoiceID, CONVERT(VARCHAR(10), InvoiceDate, 101) AS InvoiceDate, 
				CONVERT(VARCHAR(10), DeliveryDate, 101) AS DeliveryDate, InvoiceTotal, 
				InvoiceTaxAmount, InvoiceAdjustment
			FROM dbo.PricingInvoiceHeader Header 
			WHERE 
				AuditKey = @AuditKey AND 
				InvoiceID = @InvoiceID
			FOR XML PATH(''), TYPE 
		) AS Header, 
		(
			SELECT LineItemNumber, QuantityReceived, Price, Extension, VendorItemCode, ItemName
			FROM dbo.PricingInvoiceDetail Detail 
			WHERE 
				AuditKey = @AuditKey AND 
				InvoiceID = @InvoiceID
			ORDER BY LineItemNumber
			FOR XML AUTO, ELEMENTS, TYPE
		) 
FOR XML PATH('Invoice'), ROOT('Invoices') 
) AS XML_COL</property>
