                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH KronosTotalHours AS (
	SELECT EmployeeKey
		,RestaurantKey
		,EventDate
		,PunchInDate
		,PunchOutDate
		,ISNULL(JobCode, '') AS JobCode
	FROM ETL.KronosTimeCard
)

, AlohaTotalHours AS (
	SELECT 
		EmployeeKey
		,RestaurantKey
		,EventDate
		,PunchInDate
		,PunchOutDate
		,ISNULL(JobCode, '') AS JobCode
	FROM ETL.AlohaTimeCard
)

SELECT  COALESCE(KR.EmployeeKey,AL.EmployeeKey) AS EmployeeKey
	,COALESCE(KR.RestaurantKey,AL.RestaurantKey) AS RestaurantKey
	,COALESCE(KR.EventDate,AL.EventDate) AS EventDate
	,CAST(CONVERT(VARCHAR(10),COALESCE(KR.EventDate,AL.EventDate),112) AS INT) AS EventDateKey
	,KR.PunchInDate AS InPunchTime
	,KR.JobCode AS KronosJobCode
	,AL.JobCode AS AlohaJobCode
FROM KronosTotalHours KR
FULL OUTER JOIN AlohaTotalHours AL
	ON AL.EmployeeKey = KR.EmployeeKey
	AND AL.RestaurantKey = KR.RestaurantKey
	AND AL.PunchInDate = KR.PunchInDate
WHERE KR.JobCode &lt;&gt; AL.JobCode</property>
