/**************************************************************/
--Aloha Checkout Time
--Gets the Last Checkout time on each day for each employee
/**************************************************************/
;
WITH AlohaCheckout AS(	
	SELECT DateOfBusiness,
		FKStoreId,
		FKEmployeeNumber,
		MAX(OrderDateTime) AS CheckOutDateTime
	FROM   Aloha.HstGndSale (NOLOCK)
	WHERE  [TYPE] = 22
	GROUP BY DateOfBusiness,
		FKStoreId,
		FKEmployeeNumber
)

/**************************************************************/
--Aloha Punches 
/**************************************************************/
, AlohaPunch AS (
	SELECT  
		GRD.UniqueId AS AlohaGrindId
		,EMP.SSN AS EmployeeId
		,GRD.FKStoreId
		,GRD.FKEmployeeNumber
		,ISNULL(EmployeeKey,-1) AS EmployeeKey
		,EMP.Owner AS RestaurantId
		,ISNULL(DR.RestaurantKey,-1) AS RestaurantKey
		,CAST(GRD.DateOfBusiness AS DATE) AS EventDate
		,CAST(CONVERT(VARCHAR(10),GRD.DateOfBusiness,112) AS INT) EventDateKey
		,PunchInDateTime
		,PunchOutDateTime
		,CASE	WHEN CAST(AC.CheckOutDateTime AS TIME) BETWEEN '2:44' AND '3:29' THEN NULL 
				ELSE AC.CheckOutDateTime 
			END AS CheckOutDateTime
		,CASE	WHEN  CAST(PunchOutDateTime AS TIME) BETWEEN '2:44' AND '3:29' THEN 1					
				ELSE 0 
			END AS MissingOutPunchFlag
		,TotalMinutes AS AlohaShiftTimeMin
		,TM_IN.Time AS PunchInTime
		,TM_O.Time AS PunchOutTime
		,TM_IN.TimeKey AS PunchInTimeKey
		,TM_O.TimeKey AS PunchOutTimeKey
		,GRD.FKJobCodeId AS JobCode
		,'Shift' AS AlohaSource
	FROM Aloha.HstGndShift GRD (NOLOCK)
	INNER JOIN Aloha.EmployeeByStore EMP (NOLOCK)
		ON	GRD.FKEmployeeNumber = EMP.EmployeeNumber
		AND GRD.FKStoreId = EMP.Owner
	INNER JOIN Dim.Time TM_IN (NOLOCK)
		ON GRD.InHour = TM_IN.Hour
		AND GRD.InMinute = TM_IN.MinuteName
		AND TM_IN.SecondsName = '00'
	INNER JOIN Dim.Time TM_O  (NOLOCK)
		ON GRD.OutHour = TM_O.Hour
		AND GRD.OutMinute = TM_O.MinuteName
		AND TM_O.SecondsName = '00'
	LEFT OUTER JOIN Dim.Restaurant DR  (NOLOCK)
		ON TRY_CONVERT(INT,EMP.Owner) = DR.RestaurantId
		AND GRD.DateOfBusiness BETWEEN DR.audRowBeginDate AND DR.audRowEndDate
	LEFT OUTER JOIN Dim.Employee DEMP (NOLOCK)
		ON EMP.SSN  = DEMP.EmployeeIdInt
	LEFT OUTER JOIN AlohaCheckout AC
		ON AC.FKStoreId = GRD.FKStoreId
		AND AC.FKEmployeeNumber = GRD.FKEmployeeNumber
		AND AC.DateOfBusiness = GRD.DateOfBusiness  
	WHERE FKJobCodeId NOT IN(50,59) --Salary Employees, MIT
	AND CAST(GRD.DateOfBusiness AS DATE) &gt;= ?
	--Do Not Include Grind Break.  The TotalMinutes should factor in the Break time
	--FROM Aloha.HstGndBreak GRD
	
)

/**************************************************************/
--Kronos Punches
--Aloha Out Punches that have a matching Kronos Punch will not be counted out missing out punches.
/**************************************************************/
,KronosPunchEvent AS (
		SELECT DISTINCT
			VE.PERSONNUM_INT,
			PU1.PUNCHDTM    
			--LA1.RestaurantId,
			--LA1.JobCode,
			--POV1.SHORTNM PUNCHOVERRIDE 
		FROM Kronos.PUNCHEVENT PU1  (NOLOCK)
		JOIN Kronos.WTKEMPLOYEE WE (NOLOCK) ON (PU1.EMPLOYEEID =  WE.EMPLOYEEID) 
		JOIN Kronos.PERSON VE (NOLOCK) ON   (WE.PERSONID = VE.PERSONID) 
		LEFT OUTER JOIN  Kronos.LABORACCT LA1 (NOLOCK) ON (PU1.LABORACCTID = LA1.LABORACCTID )
		LEFT OUTER JOIN Kronos.OVERRIDETYPE POV1 (NOLOCK) ON (PU1.OVERRIDETYPEID = POV1.OVERRIDETYPEID)
		WHERE POV1.SHORTNM  = 'Out Punch'
)  


/**************************************************************/
--If the Out Punch is Flagged as a missing punch above AND it is not found in Kronos then it is a missing Aloha Punch 
--MissingOutPunchFlag
	--PunchOutDateTime BETWEEN '2:44' AND '3:00'
	--KP.PERSONNUM_INT IS NULL
	--CheckOutDateTime = PunchOutDateTime
/**************************************************************/
	SELECT 
		AlohaGrindId
		,EmployeeId
		,EmployeeKey
		,AP.RestaurantId
		,RestaurantKey
		,EventDate
		,EventDateKey
		,PunchInDateTime
		,CASE	WHEN MissingOutPunchFlag = 1  AND KP.PERSONNUM_INT IS NULL THEN NULL 
				ELSE PunchOutDateTime 
			END AS PunchOutDateTime
		,CheckOutDateTime
		,CASE	WHEN ISNULL(CheckOutDateTime,'1/1/1900') = ISNULL(PunchOutDateTime,'12/31/9999') THEN 1 ELSE 0 END AS CheckMatchedOutPunchFlag 
		,CASE	WHEN KP.PERSONNUM_INT IS NOT NULL THEN 0 
				ELSE MissingOutPunchFlag 
			END AS MissingOutPunchFlag
		,CASE	WHEN   KP.PERSONNUM_INT IS NULL THEN 0 ELSE 1 END AS OutPunchFoundInKronosFlag
		,CASE	WHEN MissingOutPunchFlag = 1  AND KP.PERSONNUM_INT IS NULL THEN NULL 
				ELSE AlohaShiftTimeMin 
			END AS AlohaShiftTimeMin
		,PunchInTime
		,PunchInTimeKey
		,CASE WHEN MissingOutPunchFlag = 1 AND KP.PERSONNUM_INT IS NULL THEN NULL ELSE PunchOutTime END AS PunchOutTime
		,CASE WHEN MissingOutPunchFlag = 1 AND KP.PERSONNUM_INT IS NULL THEN -1 ELSE PunchOutTimeKey END AS PunchOutTimeKey
		,JobCode
		,AlohaSource
		,ROW_NUMBER() OVER (PARTITION BY EmployeeId,EventDate ORDER BY PunchInDateTime) AS PunchOfTheDay
		,CASE	WHEN MissingOutPunchFlag = 1 AND KP.PERSONNUM_INT IS NULL THEN NULL 
				ELSE DATEDIFF(ss,PunchInDateTime,PunchOutDateTime) 
			END AS ShiftTimeSec
		,CASE WHEN MissingOutPunchFlag = 1 AND KP.PERSONNUM_INT IS NULL THEN NULL
				ELSE DATEDIFF(mi,PunchInDateTime,PunchOutDateTime) 
			END AS ShiftTimeMin
		,CASE	WHEN MissingOutPunchFlag = 1 AND KP.PERSONNUM_INT IS NULL THEN NULL
				ELSE DATEDIFF(hh,PunchInDateTime,PunchOutDateTime) 
			END AS ShiftTimeHours
	FROM AlohaPunch AP
	LEFT OUTER JOIN KronosPunchEvent KP
		ON	AP.EmployeeId = KP.PERSONNUM_INT
		AND AP.PunchOutDateTime = KP.PUNCHDTM</property>
