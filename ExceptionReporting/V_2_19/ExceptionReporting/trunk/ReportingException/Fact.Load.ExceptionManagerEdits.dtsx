                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">;WITH KronosPunch AS (

	SELECT 
		VP.PERSONNUM AS EmployeeId
		,ISNULL(EmployeeKey,-1) AS EmployeeKey
		,VP.TIMESHEETITEMID AS KronosTimesheetId
		,VP.WFCTOTALID
		,VP.LABORACCTID
		,VP.RestaurantId
		,ISNULL(DR.RestaurantKey,-1) AS RestaurantKey
		,VP.JobCode
		,CAST(VP.ADJUSTEDAPPLYDATE AS DATE) AS EventDate
		,CAST(CONVERT(VARCHAR(10),VP.ADJUSTEDAPPLYDATE,112) AS INT) EventDateKey
		,VP.STARTDTM				AS PunchInDate
		,VP.ENDDTM					AS PunchOutDate
		,DATEDIFF(mi,VP.STARTDTM,VP.ENDDTM) AS ShiftTimeMinCALC
		,VP.TIMEINSECONDS			AS KronosShiftTimeSec
		,VP.TIMEINSECONDS / 60.00	AS KronosShiftTimeMin
		,DATEDIFF(mi,VP.STARTDTM,VP.ENDDTM) AS ShiftTimeMin
		,TM_IN.Time AS PunchInTime
		,TM_O.Time AS PunchOutTime
		,TM_IN.TimeKey AS PunchInTimeKey
		,TM_O.TimeKey AS PunchOutTimeKey
	FROM Kronos.VP_TOTALS	VP (NOLOCK)
	LEFT OUTER JOIN Dim.Time TM_IN (NOLOCK)
		ON CAST(VP.STARTDTM AS TIME) = TM_IN.Time
	LEFT OUTER JOIN Dim.Time TM_O  (NOLOCK)
		ON CAST(VP.ENDDTM AS TIME) = TM_O.Time
	LEFT OUTER JOIN dim.Employee	dEMP  (NOLOCK) ON VP.PERSONNUM = dEMP.[EmployeeId]
	LEFT OUTER JOIN dim.Restaurant	dR	 (NOLOCK) 
		ON	VP.[RestaurantId] = dR.[RestaurantId]
		AND	VP.ADJUSTEDAPPLYDATE BETWEEN DR.audRowBeginDate AND DR.audRowEndDate
	WHERE PAYCODENAME = 'Regular Stores' 
	AND	APPLYDATE = ADJUSTEDAPPLYDATE
	AND APPLYDATE &gt;= ?
)


	SELECT *
		,ROW_NUMBER() OVER (PARTITION BY EmployeeId,EventDate order by PunchInDate) AS PunchOfTheDay
	FROM KronosPunch</property>
