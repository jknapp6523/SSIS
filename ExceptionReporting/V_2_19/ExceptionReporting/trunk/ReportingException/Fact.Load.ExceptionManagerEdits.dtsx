                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">/**************************************************************/
--Manager Edits - Load Fact - Aloha 
/**************************************************************/

;WITH AlohaTimeAgg AS (
	SELECT ATC.EmployeeKey, ATC.RestaurantKey, ATC.EventDateKey, ATC.EmployeeId
		
		,MAX(CheckOutDateTime) AS MaxCheckOutDateTime
		,SUM(AlohaShiftTimeMin) AS TotalMin
		,MAX(ISNULL(PunchOutDate,'12/31/9999')) AS AlohaOutPunchTime
		,MAX(CAST(MissingOutPunchFlag AS SMALLINT)) AS MissingOutPunchFlag
		,MAX(PunchOfTheDay) AS MaxPunchOfTheDay
	FROM ETL.AlohaTimeCard ATC
	GROUP By ATC.EmployeeKey, ATC.RestaurantKey, ATC.EventDateKey, ATC.EmployeeId
)

SELECT AGG.EmployeeKey
	,AGG.RestaurantKey
	,AGG.EventDateKey
	,AGG.EmployeeId
	,CASE WHEN MaxCheckOutDateTime = AlohaOutPunchTime THEN 1 ELSE 0 END AS CheckOutMatchedOutPunchFlag
	,MaxCheckOutDateTime
	,CASE 
		WHEN ISNULL(ATC.OutPunchFoundInKronosFlag,0) = 1 THEN TotalMin
		WHEN MaxCheckOutDateTime = AlohaOutPunchTime THEN NULL 
		WHEN AGG.MissingOutPunchFlag = 1 THEN NULL
		ELSE TotalMin 
	 END AS AlohaTotalMin
	,AlohaOutPunchTime
	,CASE 
		WHEN ISNULL(ATC.OutPunchFoundInKronosFlag,0) = 1 THEN 0
		WHEN MaxCheckOutDateTime = AlohaOutPunchTime THEN 1 
		ELSE AGG.MissingOutPunchFlag 
	 END AS MissingOutPunchFlag
	 ,ISNULL(ATC.OutPunchFoundInKronosFlag,0) AS OutPunchFoundInKronosFlag
FROM AlohaTimeAgg AGG
LEFT OUTER JOIN ETL.AlohaTimeCard ATC
	ON  AGG.EmployeeKey = ATC.EmployeeKey
	AND AGG.RestaurantKey = ATC.RestaurantKey
	AND AGG.EventDateKey = ATC.EventDateKey
	AND AGG.MaxPunchOfTheDay = ATC.PunchOfTheDay

ORDER BY AGG.EmployeeKey,AGG.RestaurantKey,AGG.EventDateKey</property>
