--Exception 18 - Time Sheet Approvals - BaseHours
/************************************************************************/

DECLARE @HistoryBeginDate DATE
	,@ReloadHistoryFlag TINYINT = 0

;WITH PayRollMonday AS (
	SELECT  TOP 7 DD.DATE
	FROM Dim.DATE DD WITH (NOLOCK)
	WHERE DD.DATE &lt;= GETDATE()
		AND DD.PayRollMondayFlag = 'Y'
		AND DD.DateKey &lt;&gt; - 1
	ORDER BY Date DESC)

SELECT @HistoryBeginDate = MIN(Date) 
FROM PayRollMonday

;
WITH PayStartPeriodDate
AS (
	SELECT DATE AS PayPeriodStart
	FROM Dim.DATE (NOLOCK) DD
	WHERE DATE &lt; CAST(GETDATE() AS DATE)
		AND DD.PayRollMondayFlag = 'Y'
		AND DD.DateKey &lt;&gt; - 1
	)
	
,PayEndPeriodDate AS (
	SELECT DATEADD(DD, - 1, DATE) AS PayPeriodEnd
	FROM DIM.DATE (NOLOCK) DD
	WHERE DATE &lt; CAST(GETDATE() AS DATE)
		AND DD.PayRollMondayFlag = 'Y'
		AND datekey &lt;&gt; - 1
	)

,PayPeriod AS (
	SELECT CAST(PayPeriodStart AS DATETIME) AS PayPeriodStart,
		CAST(MIN(PayPeriodEnd) AS DATETIME) AS PayPeriodEnd
	FROM PayStartPeriodDate
	CROSS JOIN PayEndPeriodDate
	WHERE PayPeriodStart &lt; PayPeriodEnd
	GROUP BY PayPeriodStart
)

SELECT PP.PayPeriodStart,
	PP.PayPeriodEnd,
	TM.HomeRestaurant AS RestaurantId,
	TM.TMID,
	TM.HomeJobCode AS Position,
	TM.PayCycle,
	TM.DateOfHire
FROM Stage.vwKronosTeamMember TM (NOLOCK) 
CROSS JOIN PayPeriod PP
WHERE TM.PayCycle NOT IN ('TERM','TRMNY')
	AND TM.LL1 &lt;&gt; 'CORP'
	AND TM.STATUS = 'Active'
	AND TM.DateOfHire &lt;= PP.PayPeriodEnd
	AND PP.PayPeriodStart &gt;= (
		SELECT MIN(TRCENTEREDONDTM)
		FROM Kronos.VP_SIGNOFFAUDITException(NOLOCK)
		)
ORDER BY TM.TMID, PP.PayPeriodEnd</property>
