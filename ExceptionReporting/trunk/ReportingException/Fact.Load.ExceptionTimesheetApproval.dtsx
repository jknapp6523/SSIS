--SignOffAudit for all 51 TMs
/************************************************************************/
	;
WITH PayStartPeriodDate AS (
	SELECT DATE AS PayPeriodStart
	FROM Dim.DATE (NOLOCK) DD
	WHERE DATE &lt; CAST(GETDATE() AS DATE)
		AND DD.PayRollMondayFlag = 'Y'
		AND DD.DateKey &lt;&gt; - 1
	)
,PayEndPeriodDate AS (
	SELECT DATEADD(DD, - 1, DATE) AS PayPeriodEnd
	FROM DIM.DATE (NOLOCK) DD
	WHERE DATE &lt; CAST(GETDATE() AS DATE)
		AND DD.PayRollMondayFlag = 'Y'
		AND datekey &lt;&gt; - 1
	)
,PayPeriod AS (
	SELECT CAST(PayPeriodStart AS DATETIME) AS PayPeriodStart,
		CAST(MIN(PayPeriodEnd) AS DATETIME) AS PayPeriodEnd
	FROM PayStartPeriodDate
	CROSS JOIN PayEndPeriodDate
	WHERE PayPeriodStart &lt; PayPeriodEnd
	GROUP BY PayPeriodStart
	)
,BaseHours AS (
	SELECT DISTINCT VT.PERSONNUM_INT AS TMID,
		PP.PayPeriodEnd
	FROM KRONOS.VP_TOTALS VT WITH (NOLOCK)
	INNER JOIN PayPeriod PP
		ON  VT.APPLYDATE BETWEEN PP.PayPeriodStart
		AND PP.PayPeriodEnd
	WHERE PAYCODENAME = 'BASE HOURS'
		AND ISNUMERIC(VT.PERSONNUM) = 1
		AND TIMEINSECONDS &lt;&gt; 0
		AND ADJUSTEDAPPLYDATE = APPLYDATE
	)
	
,SignOffAudit51 AS (
	SELECT SA.PERSONNUM_INT AS TMID,
		R.RestaurantKey,
		TM.HomeRestaurant  AS RestaurantID,
		SA.WFCAUDITTYPE,
		TM.PayCycle,
		SA.WFCAUDITFROMDTM,
		SA.WFCAUDITTODTM,
		PP.PayPeriodStart,
		PP.PayPeriodEnd,
		DATEADD(HH, 10, DATEADD(d, 1, PP.PayPeriodEnd)) AS PayPeriodEndLate,
		TRY_CONVERT(DATETIME, TRCENTEREDONDTM) AS TRCENTEREDONDTM,
		CASE WHEN TimeZoneName = 'Arizona'
				THEN DATEADD(HH, CASE WHEN DaylightSavingsFlag = 'Y' THEN - 1 ELSE 0 END, TRCENTEREDONDTM)
				ELSE DATEADD(HH, - R.ServerOffsetHours, TRCENTEREDONDTM)
			END AS ServerEnteredOnDate,
		SA.CLIENTUSERNAME,
		TM.DateOfHire,
		TM2.HOMELABORLEVELNAME5 AS LL5,
		TM.HomeJobCode AS Position,
		TM.STATUS AS TMStatus,
		ROW_NUMBER() OVER (
			PARTITION BY SA.PERSONNUM,
			TM.PayCycle,
			PP.PayPeriodEnd ORDER BY SA.TRCENTEREDONDTM
			) AS Rn
	FROM Kronos.VP_SIGNOFFAUDITException SA WITH (NOLOCK)
	INNER JOIN PayPeriod PP
		ON SA.WFCAUDITTODTM BETWEEN PP.PayPeriodStart AND PP.PayPeriodEnd
	LEFT JOIN Dim.DATE D
		ON CAST(TRCENTEREDONDTM AS DATE) = D.DATE
	INNER JOIN Stage.vwKronosTeamMember TM WITH (NOLOCK)
		ON SA.PERSONNUM_INT = TM.TMID
	INNER JOIN Kronos.VP_PERSON TM2 WITH (NOLOCK)
		ON SA.CLIENTUSERNAME = TM2.USERACCOUNTNM
	LEFT JOIN Dim.Restaurant R WITH (NOLOCK)
		ON TM.HomeRestaurant = R.RestaurantID
			AND R.audRowActiveFlag = 'Y'
	WHERE TM.PayCycle NOT IN ('TERM','TRMNY')
		AND TM.LL1 &lt;&gt; 'CORP'
		AND TM.STATUS = 'Active'
		AND TM.HomeJobCode = 51
		AND R.CompanyId &lt;&gt; 'RRBW'
		--Exclude PayRoll (PR) employees
		AND TM2.HOMELABORLEVELNAME5 &lt;&gt; 'PR'
		AND SA.WFCAUDITTYPE &lt;&gt; 'Remove supervisor approval'

	)

SELECT SA.*
FROM SignOffAudit51 SA
--added this join to restrict approvals to those TMs that were paid in the pay period
INNER JOIN BaseHours BH	
	ON  SA.TMID = BH.TMID
	AND SA.PayPeriodEnd = BH.PayPeriodEnd
WHERE Rn = 1</property>
