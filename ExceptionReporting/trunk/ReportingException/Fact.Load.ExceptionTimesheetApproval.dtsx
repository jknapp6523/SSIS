                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">/************************************************************************/
--Exception 18 - Time Sheet Approvals
/************************************************************************/

WITH SignOffAuditFull AS (
	SELECT COALESCE(SA.TMID, TM.TMID) AS TMID,
		COALESCE(SA.RestaurantId, TM.RestaurantId) AS RestaurantId,
		SA.WFCAUDITTYPE,
		COALESCE(SA.PayCycle, TM.PayCycle) AS PayCycle,
		SA.WFCAUDITFROMDTM,
		SA.WFCAUDITTODTM,
		CONVERT(DATE, COALESCE(SA.PayPeriodStart, TM.PayPeriodStart)) AS PayPeriodStart,
		CONVERT(DATE, COALESCE(SA.PayPeriodEnd, TM.PayPeriodEnd)) AS PayPeriodEnd,
		--Go to 10AM MST on the Day after PayPeriodEnd
		DATEADD(HH, 10, DATEADD(d, 1, COALESCE(SA.PayPeriodEnd, TM.PayPeriodEnd))) AS PayPeriodEndLate, 
		SA.TRCENTEREDONDTM,
		SA.ServerEnteredOnDate,
		SA.CLIENTUSERNAME,
		COALESCE(SA.Position, TM.Position) AS Position,
		COALESCE(SA.DateOfHire, TM.DateOfHire) AS DateOfHire
	FROM ETL.SignOffAuditFull TM
	LEFT JOIN Stage.SignOffAudit(NOLOCK) SA
		ON  TM.TMID = SA.TMID
		AND TM.PayPeriodEnd = SA.PayPeriodEnd
)

SELECT CONVERT(DATE, PayPeriodStart) AS PayPeriodStart
	,CONVERT(DATE, PayPeriodEnd) AS PayPeriodEnd
	,R.RestaurantKey
	,R.RestaurantId
	,COUNT(*) AS TotalCount
	,SUM(CASE 
			WHEN TRCENTEREDONDTM IS NULL THEN 0
			WHEN WFCAUDITTYPE = 'Remove supervisor approval'THEN 0
			ELSE 1
		END) AS ApprovedCount
	,COUNT(*) - SUM(CASE 
			WHEN TRCENTEREDONDTM IS NULL THEN 0
			WHEN WFCAUDITTYPE = 'Remove supervisor approval' THEN 0
			ELSE 1
		END) AS NotApprovedCount
	,SUM(CASE 
			WHEN CLIENTUSERNAME &lt;&gt; 'Payroll' THEN 1
			ELSE 0
		END) AS NonPayRollCount
	,SUM(CASE 
			WHEN Position = 51 THEN 1
			WHEN CLIENTUSERNAME = 'Payroll' THEN 1
			ELSE 0
		END) AS PayRollCount
	,SUM(CASE 
			WHEN ServerEnteredOnDate &gt; PayPeriodEndLate	THEN 1
			ELSE 0
		END) AS LateCount
	,SUM(CASE 
			WHEN ServerEnteredOnDate &lt;= PayPeriodEndLate THEN 1
			ELSE 0
		END) AS OnTimeCount
FROM SignOffAuditFull SO
INNER JOIN Dim.Restaurant (NOLOCK) R
	ON SO.RestaurantId = R.RestaurantID 
	AND SO.PayPeriodEnd BETWEEN R.audRowBeginDate AND R.audRowEndDate
WHERE R.CompanyId &lt;&gt; 'RRBW'
GROUP BY PayPeriodStart,
	PayPeriodEnd,
	R.RestaurantKey,
	R.RestaurantId</property>
