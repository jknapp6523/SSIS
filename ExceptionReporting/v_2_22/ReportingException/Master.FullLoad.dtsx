                Name="ScriptMain.vb"
                Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
	

    ' ****************************************************************************************
    ' DESCRIPTION: Retrieve multiple System variables from parent package (calling package)
    ' OnError event handler to build the vErrorMessage that will be inserted in
    ' the Audit.ExecutionLog database FailureMessage column
    ' ****************************************************************************************

    Public Sub Main()

        'StringBuilder used to build the error message
        Dim sbError As New System.Text.StringBuilder()

        ' Initialize the task result to success
        Dts.TaskResult = ScriptResults.Success
        ' Mark initial progress
        'Dts.Events.FireProgress("Building error message string", 0, 0, 0, "SCP Build Error Message", True)

        'Initialize Error Message
        sbError.Append("")

        Try
            sbError.Append("(Error Code: " + Dts.Variables("ErrorCode").Value.ToString() + ") " & vbCrLf)
            sbError.Append(Dts.Variables("ErrorDescription").Value.ToString() & vbCrLf)

            If Dts.Variables("cnfErrorMessage").Value.ToString <> "" Then
                sbError.Append(vbCrLf & "--- User Defined Package Errors ---" & vbCrLf & Dts.Variables("cnfErrorMessage").Value.ToString)
            End If

            ' Set variable value to the concatenated string from the string builder
            Dts.Variables("cnfErrorMessage").Value = sbError.ToString()

            'Get the Lenth of the error message
            Dim iMaxLen As Integer = sbError.ToString().Length

            'If it is greater than 2000, set it to 2000.  
            'The email body can not be greated than 4000
            If iMaxLen > 2000 Then
                iMaxLen = 2000
            Else
                iMaxLen = iMaxLen - 1
            End If

            Dts.Variables("cnfErrorMessageShort").Value = sbError.ToString().Substring(0, iMaxLen)

            Dts.Variables("cnfSSISErrorMailSubject").Value = Dts.Variables("cnfSSISErrorMailSubject").Value & _
                " - Package:" & Dts.Variables("PackageName").Value & " - " & Dts.Variables("MachineName").Value

        Catch ex As Exception

            ' Write out the error
