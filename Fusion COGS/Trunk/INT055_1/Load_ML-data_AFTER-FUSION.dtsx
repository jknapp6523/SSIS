              description="OLE DB Source"
              name="Read from ML vItemUseVariance"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[dbo].[vItemUseVariance]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--	Get last subperiod with data

		DECLARE @SubPeriod int
		SET @SubPeriod = (SELECT MAX(SubPeriod) FROM CostOfGoodsSoldCube.dbo.ItemUseVariance)



--	Read COGS data for that subperiod forward

SELECT 
        CASE WHEN [SubPeriod] IS NULL THEN NULL ELSE CAST([SubPeriod] AS [INT])								END	AS	[SubPeriod]
      , CASE WHEN [StoreID] IS NULL THEN NULL ELSE CAST([StoreID] AS [VARCHAR](15))							END	AS	[StoreID]
      , CASE WHEN [RawMaterialItemCatNum] IS NULL THEN NULL ELSE CAST([RawMaterialItemCatNum] AS [INT])		END	AS	[RawMaterialItemCatNum]
      , CASE WHEN [RawMaterialItemName] IS NULL THEN NULL ELSE CAST([RawMaterialItemName] AS [varchar](15))	END	AS	[RawMaterialItemName]
      , CASE WHEN [IngredientID] IS NULL THEN NULL ELSE CAST([IngredientID] AS [VARCHAR](15))				END	AS	[IngredientID]
      , CASE WHEN [SumOfTheoryUse] IS NULL THEN NULL ELSE CONVERT(FLOAT, [SumOfTheoryUse])					END AS	[SumOfTheoryUse]
      , CASE WHEN [EndingItemCost] IS NULL THEN NULL ELSE CONVERT(MONEY, [EndingItemCost])					END AS	[EndingItemCost]
      , CASE WHEN [ActualUsage] IS NULL THEN NULL ELSE CONVERT(FLOAT, [ActualUsage])						END AS	[ActualUsage]
      , CASE WHEN [ActualUse$] IS NULL THEN NULL ELSE CONVERT(FLOAT, [ActualUse$])							END AS	[ActualUse$]
      , CASE WHEN [Variance] IS NULL THEN NULL ELSE CONVERT(FLOAT, [Variance])								END AS	[Variance]
      , CASE WHEN [IdealUse$] IS NULL THEN NULL ELSE CONVERT(FLOAT, [IdealUse$])							END AS	[IdealUse$]
      , CASE WHEN [Variance$] IS NULL THEN NULL ELSE CONVERT(FLOAT, [Variance$])							END AS	[Variance$]
  FROM [ML].[dbo].[vItemUseVariance]
 WHERE [SubPeriod] &gt;=	@SubPeriod


 ----imp</property>
