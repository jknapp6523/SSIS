                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @PIPOMondayDateRule INT 
DECLARE @DateWeekStart DATETIME 
DECLARE @DateWeekEnd DATETIME 

SET @PIPOMondayDateRule = ? 
SET @DateWeekStart = ? 
SET @DateWeekEnd = ? 

SELECT a.SiteNumber, a.StoreID, a.InvoiceNumber, a.InvoiceID, a.InvoiceDate, a.VendorItemID, 
	a.InvoiceLineItem, a.QuantityReceived, a.PurchaseUnitPrice, a.ExtendedPrice, 
	a.RawMaterialItemCatID, a.RawMaterialItemCatName, a.ItemName 
FROM dbo.ST_InvoiceDetails a 
WHERE 
	EXISTS ( 
		SELECT 1 
		FROM dbo.ST_InvoiceHeaders z 
		WHERE 
			a.SiteNumber = z.SiteNumber AND 
			a.InvoiceNumber = z.InvoiceNumber AND 
			a.InvoiceDate = z.InvoiceDate AND 
			CASE 
				WHEN (UPPER(z.InvoiceType) IN ('PAID IN', 'PAID OUT') AND 
					DATEPART(WEEKDAY, z.DeliveredTime) = 2 AND 
					DATEPART(HOUR, z.DeliveredTime) &lt; 6 AND 
					@PIPOMondayDateRule = 1)  
				THEN (DATEADD(d, -1, DATEDIFF(d, 0, z.DeliveredTime))) 
				ELSE (DATEADD(d, 0, DATEDIFF(d, 0, z.DeliveredTime))) 
			END &gt;= @DateWeekStart AND 
			CASE 
				WHEN (UPPER(z.InvoiceType) IN ('PAID IN', 'PAID OUT') AND 
					DATEPART(WEEKDAY, z.DeliveredTime) = 2 AND 
					DATEPART(HOUR, z.DeliveredTime) &lt; 6 AND 
					@PIPOMondayDateRule = 1)   
				THEN (DATEADD(d, -1, DATEDIFF(d, 0, z.DeliveredTime))) 
				ELSE (DATEADD(d, 0, DATEDIFF(d, 0, z.DeliveredTime))) 
			END &lt;= @DateWeekEnd)</property>
