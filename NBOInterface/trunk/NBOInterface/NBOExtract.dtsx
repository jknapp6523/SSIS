                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @PIPOMondayDateRule INT 
DECLARE @DateWeekStart DATETIME 
DECLARE @DateWeekEnd DATETIME 

SET @PIPOMondayDateRule = ? 
SET @DateWeekStart = ? 
SET @DateWeekEnd =? 

SELECT a.SiteNumber, a.StoreID, a.InvoiceNumber, a.InvoiceID, a.VendorNumber, a.VendorID, 
	a.TotalAmountPayable, a.InvoiceDate, a.FreightAmount, a.SalesTaxAmount, a.DeliveredTime, 
	a.InvoiceType
FROM ( 
	SELECT aa.SiteNumber, aa.StoreID, aa.InvoiceNumber, aa.InvoiceID, aa.VendorNumber, aa.VendorID, 
		aa.TotalAmountPayable, aa.InvoiceDate, aa.FreightAmount, aa.SalesTaxAmount, 
		CASE 
			WHEN (UPPER(aa.InvoiceType) IN ('PAID IN', 'PAID OUT') AND 
				DATEPART(WEEKDAY, aa.DeliveredTime) = 2 AND 
				DATEPART(HOUR, aa.DeliveredTime) &lt; 6 AND 
				@PIPOMondayDateRule = 1) 
			THEN (DATEADD(d, -1, DATEDIFF(d, 0, aa.DeliveredTime))) 
			ELSE (DATEADD(d, 0, DATEDIFF(d, 0, aa.DeliveredTime))) 
		END AS DeliveredTime, aa.InvoiceType
	FROM dbo.ST_InvoiceHeaders aa) a  
WHERE 
	a.DeliveredTime &gt;= @DateWeekStart AND 
	a.DeliveredTime &lt;= @DateWeekEnd</property>
