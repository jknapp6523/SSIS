          SQLTask:Connection="{09E4493E-9FC8-45E1-BA07-BCBE3E51D402}"
          SQLTask:SqlStatementSource="--DECLARE AND SET&#xA;DECLARE @JobName NVARCHAR(256) &#xA;DECLARE @AuditKey INT &#xA;DECLARE @SessionRequestedSource INT &#xA;DECLARE @FolderFintechMapping VARCHAR(4000) &#xA;DECLARE @FolderFintechMappingArchive VARCHAR(4000) &#xA;&#xA;&#xA;SET @JobName = ? &#xA;&#xA;SET @AuditKey = (&#xA;&#x9;SELECT MAX(a.AuditKey) AS AuditKey &#xA;&#x9;FROM audit.Session a &#xA;&#x9;WHERE a.JobName = @JobName AND &#xA;&#x9;&#x9;a.IsCurrentSession = 1) &#xA;&#xA;SET @SessionRequestedSource = ( &#xA;&#x9;SELECT a.SessionRequestedSource &#xA;&#x9;FROM audit.Session a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.AuditKey = @AuditKey)&#xA;&#xA;&#xA;--SET VALUES &#xA;IF @SessionRequestedSource = 1&#xA;&#x9;--SCHEDULED FROM AGENT &#xA;&#x9;BEGIN &#xA;&#xA;&#x9;&#x9;--@FolderFintechMapping &#xA;&#x9;&#x9;SET @FolderFintechMapping = (&#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue + &#xA;&#x9;&#x9;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN (RIGHT(a.ConfiguredValue, 1) = '\') THEN ('') &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE ('\') &#xA;&#x9;&#x9;&#x9;&#x9;END  &#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE&#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder Fintech Mapping')&#xA;&#xA;&#x9;&#x9;--@FolderFintechMappingArchive &#xA;&#x9;&#x9;SET @FolderFintechMappingArchive = (&#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue + &#xA;&#x9;&#x9;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN (RIGHT(a.ConfiguredValue, 1) = '\') THEN ('') &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE ('\') &#xA;&#x9;&#x9;&#x9;&#x9;END  &#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE&#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder Fintech Mapping Archive')&#xA;&#x9;END &#xA;ELSE &#xA;&#x9;--NOT SCHEDULED (DEBUG OR MANUAL EXECUTION FROM AGENT) &#xA;&#x9;BEGIN &#xA;&#xA;&#x9;&#x9;--@FolderFintechMapping &#xA;&#x9;&#x9;SET @FolderFintechMapping = (&#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue + &#xA;&#x9;&#x9;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN (RIGHT(a.ConfiguredValue, 1) = '\') THEN ('') &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE ('\') &#xA;&#x9;&#x9;&#x9;&#x9;END  &#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE&#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder Fintech Mapping Manual')&#xA;&#xA;&#x9;&#x9;--@FolderFintechMappingArchive &#xA;&#x9;&#x9;SET @FolderFintechMappingArchive = (&#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue + &#xA;&#x9;&#x9;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN (RIGHT(a.ConfiguredValue, 1) = '\') THEN ('') &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE ('\') &#xA;&#x9;&#x9;&#x9;&#x9;END  &#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE&#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder Fintech Mapping Archive Manual')&#xA;&#x9;END &#xA;&#xA;&#xA;--RETURN &#xA;SELECT @AuditKey AS AuditKey, @FolderFintechMapping AS FolderFintechMapping, &#xA;&#x9;@FolderFintechMappingArchive AS FolderFintechMappingArchive"
