    <DTS:Executable
      DTS:refId="Package\Get Store Email Values"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{0D3936F3-F1C5-4AA0-9F49-A3617BF8210E}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Get Store Email Values"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{A5542CE5-B157-446B-8B43-A61C3E7DBBC6}"
          SQLTask:SqlStatementSource="---------------------------------------------------------------------------------------------------&#xA;--&#x9;INITIALIZE&#xA;---------------------------------------------------------------------------------------------------&#xA;&#xA;--DECLARE AND SET &#xA;DECLARE @i INT &#xA;DECLARE @t INT &#xA;DECLARE @x INT &#xA;DECLARE @y INT &#xA;DECLARE @AuditKey INT &#xA;DECLARE @EmailDistro VARCHAR(4000) &#xA;DECLARE @EmailBody VARCHAR(4000) &#xA;DECLARE @StoreID NVARCHAR(25) &#xA;DECLARE @BusinessDate DATETIME &#xA;DECLARE @NumericValue MONEY &#xA;&#xA;SET @AuditKey = ? &#xA;&#xA;&#xA;--CREATE TEMP TABLES &#xA;IF OBJECT_ID('tempdb..#EmailResultSet') IS NOT NULL &#xA;&#x9;DROP TABLE #EmailResultSet &#xA;  &#xA;CREATE TABLE #EmailResultSet ( &#xA;&#x9;EmailResultSetKey INT IDENTITY(1, 1) NOT NULL, &#xA;&#x9;EmailFrom VARCHAR(4000) NOT NULL, &#xA;&#x9;EmailDistro VARCHAR(4000) NOT NULL, &#xA;&#x9;EmailSubject VARCHAR(4000) NOT NULL, &#xA;&#x9;EmailBody VARCHAR(4000) NOT NULL, &#xA;&#x9;EmailPriorty INT DEFAULT(0) NOT NULL, --0 = Normal, 1 = High, 4 = Low &#xA;&#x9;EmailAttachment VARCHAR(4000) NOT NULL, &#xA;&#x9;PRIMARY KEY CLUSTERED (EmailResultSetKey)) &#xA;&#xA;IF OBJECT_ID('tempdb..#Stores') IS NOT NULL &#xA;&#x9;DROP TABLE #Stores&#xA;&#xA;CREATE TABLE #Stores ( &#xA;&#x9;StoresKey INT IDENTITY(1, 1) NOT NULL, &#xA;&#x9;StoreID NVARCHAR(25) NOT NULL, &#xA;&#x9;EmailDistro VARCHAR(4000) NOT NULL, &#xA;&#x9;PRIMARY KEY CLUSTERED (StoresKey), &#xA;&#x9;UNIQUE (StoreID))&#xA;&#xA;IF OBJECT_ID('tempdb..#BusinessDates') IS NOT NULL &#xA;&#x9;DROP TABLE #BusinessDates&#xA;&#xA;CREATE TABLE #BusinessDates ( &#xA;&#x9;BusinessDatesKey INT IDENTITY(1, 1) NOT NULL, &#xA;&#x9;BusinessDate DATETIME NOT NULL, &#xA;&#x9;NumericValue MONEY NULL, &#xA;&#x9;PRIMARY KEY CLUSTERED (BusinessDatesKey), &#xA;&#x9;UNIQUE (BusinessDate))&#xA;&#xA;---------------------------------------------------------------------------------------------------&#xA;--&#x9;STORE ALERTS &#xA;---------------------------------------------------------------------------------------------------&#xA;&#xA;--GET ALL STORES WITH DATA CHECK ISSUES &#xA;INSERT INTO #Stores (StoreID, EmailDistro) &#xA;SELECT a.StoreID, MAX(a.NotificationDistribution) AS EmailDistro&#xA;FROM dbo.DataCheck a &#xA;WHERE &#xA;&#x9;a.AuditKey = @AuditKey AND &#xA;&#x9;a.DataCheck IN ('BeginningSafeCount', 'EndingSafeCount', &#xA;&#x9;&#x9;'BeginningEndingSafeCountDiscrepancy') AND &#xA;&#x9;COALESCE(a.NotificationDistribution, '') &lt;&gt; ''&#xA;GROUP BY a.StoreID &#xA;ORDER BY a.StoreID; &#xA;&#xA;SET @t = @@ROWCOUNT &#xA;&#xA;&#xA;--LOOP BY STORE AND INSERT EMAILS INTO TEMP TABLE &#xA;SET @i = 1 &#xA;&#xA;WHILE @i &lt;= @t &#xA;BEGIN &#xA;&#x9;--START EMAIL BODY &#xA;&#x9;SET @EmailBody = 'Hello, ' &#xA;&#xA;&#xA;&#x9;--GET STORE ID / DISTRO  &#xA;&#x9;SELECT @StoreID = a.StoreID, @EmailDistro = a.EmailDistro&#xA;&#x9;FROM #Stores a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;StoresKey = @i&#xA;&#xA;&#xA;&#x9;--GET BUSINESS DATES (BEGINNING COUNTS) &#xA;&#x9;TRUNCATE TABLE #BusinessDates &#xA;&#xA;&#x9;INSERT INTO #BusinessDates (BusinessDate) &#xA;&#x9;SELECT DISTINCT a.BusinessDate&#xA;&#x9;FROM dbo.DataCheck a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.AuditKey = @AuditKey AND &#xA;&#x9;&#x9;a.DataCheck = 'BeginningSafeCount' AND &#xA;&#x9;&#x9;a.StoreID = @StoreID &#xA;&#x9;ORDER BY a.BusinessDate; &#xA;&#xA;&#x9;SET @y = @@ROWCOUNT&#xA;&#x9;SET @x = 1&#xA;&#xA;&#xA;&#x9;--SET EMAIL BODY (BEGINNING COUNTS) &#xA;&#x9;IF @y &gt; 0 &#xA;&#x9;&#x9;SET @EmailBody = @EmailBody + CHAR(13) + CHAR(13) + &#xA;&#x9;&#x9;&#x9;'The following business dates have Beginning Safe Count values of $0.00...' + CHAR(13) + CHAR(13) &#xA;&#xA;&#x9;WHILE @x &lt;= @y &#xA;&#x9;BEGIN &#xA;&#x9;&#x9;--GET BUSINESS DATE &#xA;&#x9;&#x9;SET @BusinessDate = ( &#xA;&#x9;&#x9;&#x9;SELECT a.BusinessDate &#xA;&#x9;&#x9;&#x9;FROM #BusinessDates a &#xA;&#x9;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;&#x9;a.BusinessDatesKey = @x) &#xA;&#xA;&#xA;&#x9;&#x9;--ADD BUSINESS DATE TO EMAIL BODY &#xA;&#x9;&#x9;SET @EmailBody = @EmailBody + CHAR(9) + CONVERT(VARCHAR(25), @BusinessDate, 101) + CHAR(13) &#xA;&#xA;&#xA;&#x9;&#x9;--INCREMENT &#xA;&#x9;&#x9;SET @x = @x + 1 &#xA;&#x9;END &#xA;&#xA;&#xA;&#x9;--GET BUSINESS DATES (ENDING COUNTS) &#xA;&#x9;TRUNCATE TABLE #BusinessDates &#xA;&#xA;&#x9;INSERT INTO #BusinessDates (BusinessDate) &#xA;&#x9;SELECT DISTINCT a.BusinessDate&#xA;&#x9;FROM dbo.DataCheck a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.AuditKey = @AuditKey AND &#xA;&#x9;&#x9;a.DataCheck = 'EndingSafeCount' AND &#xA;&#x9;&#x9;a.StoreID = @StoreID &#xA;&#x9;ORDER BY a.BusinessDate; &#xA;&#xA;&#x9;SET @y = @@ROWCOUNT&#xA;&#x9;SET @x = 1&#xA;&#xA;&#xA;&#x9;--SET EMAIL BODY (ENDING COUNTS) &#xA;&#x9;IF @y &gt; 0 &#xA;&#x9;&#x9;SET @EmailBody = @EmailBody + CHAR(13) + CHAR(13) + &#xA;&#x9;&#x9;&#x9;'The following business dates have Ending Safe Count values of $0.00...' + CHAR(13) + CHAR(13) &#xA;&#xA;&#x9;WHILE @x &lt;= @y &#xA;&#x9;BEGIN &#xA;&#x9;&#x9;--GET BUSINESS DATE &#xA;&#x9;&#x9;SET @BusinessDate = ( &#xA;&#x9;&#x9;&#x9;SELECT a.BusinessDate &#xA;&#x9;&#x9;&#x9;FROM #BusinessDates a &#xA;&#x9;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;&#x9;a.BusinessDatesKey = @x) &#xA;&#xA;&#xA;&#x9;&#x9;--ADD BUSINESS DATE TO EMAIL BODY &#xA;&#x9;&#x9;SET @EmailBody = @EmailBody + CHAR(9) + CONVERT(VARCHAR(25), @BusinessDate, 101) + CHAR(13) &#xA;&#xA;&#xA;&#x9;&#x9;--INCREMENT &#xA;&#x9;&#x9;SET @x = @x + 1 &#xA;&#x9;END &#xA;&#xA;&#xA;&#x9;--ADD MISSING INSTRUCTIONS &#xA;&#x9;IF @EmailBody &lt;&gt; 'Hello, ' &#xA;&#x9;&#x9;SET @EmailBody = @EmailBody + CHAR(13) + CHAR(13) + 'Please add a valid Beginning/Ending Safe Count(s) ' + &#xA;&#x9;&#x9;&#x9;'for the date(s) listed above.'  &#xA;&#xA;&#xA;&#x9;--GET BUSINESS DATES (COUNT DISCREPANCY) &#xA;&#x9;TRUNCATE TABLE #BusinessDates &#xA;&#xA;&#x9;INSERT INTO #BusinessDates (BusinessDate, NumericValue) &#xA;&#x9;SELECT a.BusinessDate, a.NumericValue&#xA;&#x9;FROM dbo.DataCheck a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.AuditKey = @AuditKey AND &#xA;&#x9;&#x9;a.DataCheck = 'BeginningEndingSafeCountDiscrepancy' AND &#xA;&#x9;&#x9;a.StoreID = @StoreID &#xA;&#x9;ORDER BY a.BusinessDate; &#xA;&#xA;&#x9;SET @y = @@ROWCOUNT&#xA;&#x9;SET @x = 1&#xA;&#xA;&#xA;&#x9;--SET EMAIL BODY (COUNT DISCREPANCY) &#xA;&#x9;IF @y &gt; 0 &#xA;&#x9;&#x9;SET @EmailBody = @EmailBody + CHAR(13) + CHAR(13) + &#xA;&#x9;&#x9;&#x9;'The following business dates have a beginning safe count different than your prior day ' + &#xA;&#x9;&#x9;&#x9;'ending safe count.  Please note, this will affect your cash over/short but will not be ' + &#xA;&#x9;&#x9;&#x9;'reflected in your 5.50 Cash Management Tool Report in NBO.' + CHAR(13) + CHAR(13) + &#xA;&#x9;&#x9;&#x9;CHAR(9) + 'Date:     ' + CHAR(9) + 'Amount: ' + CHAR(13)&#xA;&#xA;&#x9;WHILE @x &lt;= @y &#xA;&#x9;BEGIN &#xA;&#x9;&#x9;--GET BUSINESS DATE &#xA;&#x9;&#x9;SELECT @BusinessDate = a.BusinessDate, @NumericValue = a.NumericValue&#xA;&#x9;&#x9;FROM #BusinessDates a &#xA;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;a.BusinessDatesKey = @x&#xA;&#xA;&#xA;&#x9;&#x9;--ADD BUSINESS DATE TO EMAIL BODY &#xA;&#x9;&#x9;SET @EmailBody = @EmailBody + CHAR(9) + CONVERT(VARCHAR(10), @BusinessDate, 101) + &#xA;&#x9;&#x9;&#x9;CHAR(9) + '$' + CAST(FORMAT(@NumericValue, 'N', 'en-us') AS VARCHAR(25)) + CHAR(13)&#xA;&#xA;&#xA;&#x9;&#x9;--INCREMENT &#xA;&#x9;&#x9;SET @x = @x + 1 &#xA;&#x9;END &#xA;&#xA;&#xA;&#x9;--COMPLETE EMAIL BODY &#xA;&#x9;SET @EmailBody = @EmailBody + CHAR(13) + CHAR(13) + 'For questions regarding safe counts, ' + &#xA;&#x9;&#x9;'please reach out to Cash Management at cashmgmt@redrobin.com.' + &#xA;&#x9;&#x9;CHAR(13) + CHAR(13) + 'Thank you,' + CHAR(13) + CHAR(13) + CHAR(13) + CHAR(13) + 'Accounting'&#xA;&#xA;&#xA;&#x9;--INSERT &#xA;&#x9;INSERT INTO #EmailResultSet (EmailFrom, EmailDistro, EmailSubject, EmailBody, EmailPriorty, EmailAttachment) &#xA;&#x9;VALUES ('Fusion_SSIS_Support@redrobin.com', @EmailDistro, 'Safe Count Issues - Store ' + CAST(@StoreID AS VARCHAR(3)), &#xA;&#x9;&#x9;@EmailBody, 1, ''); &#xA;&#xA;&#xA;&#x9;--INCREMENT &#xA;&#x9;SET @i = @i + 1 &#xA;END &#xA;&#xA;&#xA;---------------------------------------------------------------------------------------------------&#xA;--&#x9;TERMINATE&#xA;---------------------------------------------------------------------------------------------------&#xA;&#xA;--RETURN &#xA;SELECT a.EmailFrom, a.EmailDistro, a.EmailSubject, a.EmailBody, a.EmailPriorty, a.EmailAttachment  &#xA;FROM #EmailResultSet a &#xA;ORDER BY a.EmailResultSetKey &#xA;&#xA;&#xA;--DROP TEMP TABLES &#xA;IF OBJECT_ID('tempdb..#EmailResultSet') IS NOT NULL &#xA;&#x9;DROP TABLE #EmailResultSet &#xA;&#xA;IF OBJECT_ID('tempdb..#Stores') IS NOT NULL &#xA;&#x9;DROP TABLE #Stores&#xA;&#xA;IF OBJECT_ID('tempdb..#BusinessDates') IS NOT NULL &#xA;&#x9;DROP TABLE #BusinessDates"
          SQLTask:ResultType="ResultSetType_Rowset" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="0"
            SQLTask:DtsVariableName="User::EmailResultSet" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="0"
            SQLTask:DtsVariableName="User::AuditKey"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="131"
            SQLTask:ParameterSize="-1" />
        </SQLTask:SqlTaskData>
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Get Variables"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{686A9848-0C8F-42A4-97AA-E02940B431E9}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Get Variables"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{A5542CE5-B157-446B-8B43-A61C3E7DBBC6}"
          SQLTask:SqlStatementSource="---------------------------------------------------------------------------------------------------&#xA;-- INITIALIZE &#xA;--------------------------------------------------------------------------------------------------- &#xA;&#xA;--DECLARE AND SET&#xA;DECLARE @JobName NVARCHAR(256) &#xA;DECLARE @PackageName NVARCHAR(256) &#xA;DECLARE @AuditKey INT &#xA;DECLARE @EmailDistro VARCHAR(4000) &#xA;DECLARE @EmailDistroAccounting VARCHAR(4000) &#xA;DECLARE @EmailStore INT &#xA;DECLARE @AccountingSummary INT&#xA;DECLARE @DateAccountingStart DATETIME &#xA;DECLARE @DateAccountingEnd DATETIME&#xA;DECLARE @SessionRequestedSource INT &#xA;&#xA;DECLARE @FileTimestamp VARCHAR(4000) &#xA;&#xA;DECLARE @FileOutput VARCHAR(4000) &#xA;DECLARE @FolderArchive VARCHAR(4000) &#xA;&#xA;SET @JobName = ?  &#xA;SET @PackageName = ? &#xA;&#xA;SET @AuditKey = (&#xA;&#x9;SELECT MAX(a.AuditKey) AS AuditKey &#xA;&#x9;FROM audit.Session a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.JobName = @JobName AND &#xA;&#x9;&#x9;a.IsCurrentSession = 1) &#xA;&#xA;SET @SessionRequestedSource = ( &#xA;&#x9;SELECT a.SessionRequestedSource &#xA;&#x9;FROM audit.Session a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.AuditKey = @AuditKey) &#xA;&#xA;SET @EmailDistro = COALESCE(( &#xA;&#x9;SELECT a.ConfiguredValue&#xA;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.ConfigurationFilter = 'Notification ' + @PackageName + ' Additional Distro'), '')  &#xA;&#xA;SET @EmailDistroAccounting = COALESCE(( &#xA;&#x9;SELECT a.ConfiguredValue&#xA;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.ConfigurationFilter = 'Notification ' + @PackageName + ' Accounting Distro'), '')  &#xA;&#xA;SET @EmailStore = COALESCE(( &#xA;&#x9;SELECT &#xA;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;WHEN (ISNUMERIC(a.ConfigurationValue) = 1) THEN (CAST(a.ConfigurationValue AS INT)) &#xA;&#x9;&#x9;&#x9;ELSE (0) &#xA;&#x9;&#x9;END &#xA;&#x9;FROM config.ProcessConfiguration a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.ConfigurationName = 'EmailStore' AND &#xA;&#x9;&#x9;a.ConfigurationCode = 'EndingSafeCount' ), 0) &#xA;&#xA;IF DATEPART(WEEKDAY, GETDATE()) = 2&#xA;&#x9;SET @AccountingSummary = 1 &#xA;ELSE &#xA;&#x9;SET @AccountingSummary = 0 &#xA;&#xA;SELECT @DateAccountingStart = a.StartDate, &#xA;&#x9;@DateAccountingEnd = a.EndDate&#xA;FROM nbo.ST_CalendarSubperiods a &#xA;WHERE &#xA;&#x9;a.StartDate &lt;= DATEADD(DAY, -7, GETDATE()) AND &#xA;&#x9;a.EndDate &gt;= DATEADD(DAY, -7, GETDATE()) &#xA;&#xA;SET @FileTimestamp = CONVERT(VARCHAR(4000), GETDATE(), 120) &#xA;SET @FileTimestamp = REPLACE(@FileTimeStamp, '-', '') &#xA;SET @FileTimestamp = REPLACE(@FileTimeStamp, ' ', '') &#xA;SET @FileTimestamp = REPLACE(@FileTimeStamp, ':', '') &#xA;&#xA;IF @SessionRequestedSource = 1 &#xA;&#x9;BEGIN &#xA;&#xA;&#x9;&#x9;SET @FileOutput = ( &#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue + &#xA;&#x9;&#x9;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN (RIGHT(a.ConfiguredValue, 1) &lt;&gt; '\') THEN ('\') &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE ('') &#xA;&#x9;&#x9;&#x9;&#x9;END + 'AccountingSafeCountSummary_' + @FileTimestamp + '.csv'&#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder DataCheck Output') &#xA;&#xA;&#x9;&#x9;SET @FolderArchive = ( &#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue&#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder DataCheck Archive') &#xA;&#x9;END &#xA;ELSE &#xA;&#x9;BEGIN &#xA;&#xA;&#x9;&#x9;SET @FileOutput = ( &#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue + &#xA;&#x9;&#x9;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN (RIGHT(a.ConfiguredValue, 1) &lt;&gt; '\') THEN ('\') &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE ('') &#xA;&#x9;&#x9;&#x9;&#x9;END + 'AccountingSafeCountSummary_' + @FileTimestamp + '.csv'&#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder DataCheck Output Manual') &#xA;&#xA;&#x9;&#x9;SET @FolderArchive = ( &#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue&#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder DataCheck Archive Manual') &#xA;&#x9;END &#xA;&#xA;&#x9;&#x9;&#xA;---------------------------------------------------------------------------------------------------&#xA;-- TERMINATE&#xA;---------------------------------------------------------------------------------------------------&#xA;&#xA;--RETURN &#xA;SELECT @AuditKey AS AuditKey, @EmailDistro AS EmailDistro, &#xA;&#x9;@EmailDistroAccounting AS EmailDistroAccounting, @EmailStore AS EmailStore, &#xA;&#x9;@AccountingSummary AS AccountingSummary, @DateAccountingStart AS DateAccountingStart, &#xA;&#x9;@DateAccountingEnd AS DateAccountingEnd, @FileOutput AS FileOutput, &#xA;&#x9;@FolderArchive AS FolderArchive &#xA;&#xA;"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
