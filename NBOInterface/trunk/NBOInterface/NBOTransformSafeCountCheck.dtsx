                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @AccountingSummary INT 
DECLARE @DateAccountingStart DATETIME 
DECLARE @DateAccountingEnd DATETIME 

SET @AccountingSummary = ? 
SET @DateAccountingStart = ? 
SET @DateAccountingEnd = ? 

SELECT DISTINCT a.BusinessDate, a.SiteNumber, 
	CASE 
		WHEN (UPPER(b.DataDefinitionName) = 'BEG SAFE COUNT') THEN ('BeginningSafeCountAccounting') 
		ELSE ('EndingSafeCountAccounting')
	END AS DataCheck, 
	CAST(a.AllocationAmount AS MONEY) AS NumericValue
FROM nbo.ST_Allocations a 
JOIN nbo.ST_DataDefinitionConfiguration b ON 
	a.DataDefinitionNumber = b.DataDefinitionNumber AND  
	UPPER(b.DataDefinitionName) IN ('BEG SAFE COUNT', 'END SAFE COUNT') 
WHERE 
	@AccountingSummary = 1 AND 
	a.BusinessDate BETWEEN @DateAccountingStart AND @DateAccountingEnd AND 
	a.AllocationAmount = 0 AND 
	EXISTS ( 
		SELECT 1 
		FROM config.ProcessConfiguration z 
		JOIN nbo.ST_Sites y ON 
			CAST(z.ConfigurationValue AS NVARCHAR(25)) = y.StoreID
		WHERE  
			a.SiteNumber = y.SiteNumber AND 
			a.BusinessDate &gt;= CAST(z.EffectiveDate AS DATETIME) AND 
			a.BusinessDate &lt;= CAST(z.ObsoleteDate AS DATETIME) AND 
			z.ConfigurationName = 'Fusion' AND 
			z.ConfigurationCode = 'Inclusion')</property>
