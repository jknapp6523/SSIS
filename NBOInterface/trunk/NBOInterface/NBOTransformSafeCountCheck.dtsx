            <component
              refId="Package\Transform Store Safe Counts\ST_Allocations"
              componentClassID="{165A526D-D5DE-47FF-96A6-F8274C19826B}"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="ST_Allocations"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--BEGINNING ENDING SAFE COUNT 0 
SELECT DISTINCT a.BusinessDate, a.SiteNumber, 
	CASE 
		WHEN (UPPER(b.DataDefinitionName) = 'BEG SAFE COUNT') 
		THEN (CAST('BeginningSafeCount' AS VARCHAR(100))) 
		ELSE (CAST('EndingSafeCount' AS VARCHAR(100)))
	END AS DataCheck, 
	CAST(a.AllocationAmount AS MONEY) AS NumericValue
FROM nbo.ST_Allocations a 
JOIN nbo.ST_DataDefinitionConfiguration b ON 
	a.DataDefinitionNumber = b.DataDefinitionNumber AND  
	UPPER(b.DataDefinitionName) IN ('BEG SAFE COUNT', 'END SAFE COUNT') 
WHERE 
	a.AllocationAmount = 0 AND 
	EXISTS ( 
		SELECT 1 
		FROM config.ProcessConfiguration z 
		JOIN nbo.ST_Sites y ON 
			CAST(z.ConfigurationValue AS NVARCHAR(25)) = y.StoreID
		WHERE  
			a.SiteNumber = y.SiteNumber AND 
			a.BusinessDate &gt;= CAST(z.EffectiveDate AS DATETIME) AND 
			a.BusinessDate &lt;= CAST(z.ObsoleteDate AS DATETIME) AND 
			z.ConfigurationName = 'Fusion' AND 
			z.ConfigurationCode = 'Inclusion')

UNION ALL 

--BEGINNING ENDING SAFE COUNT MISMATCH 
SELECT DISTINCT a.BusinessDate, a.SiteNumber, 
	CAST('BeginningEndingSafeCountDiscrepancy' AS VARCHAR(100)) AS DataCheck, 
	CAST((a.AllocationAmount - b.AllocationAmount) AS MONEY) AS NumericValue 
FROM (
	SELECT aa.BusinessDate, aa.SiteNumber, aa.AllocationAmount
	FROM nbo.ST_Allocations aa 
	JOIN nbo.ST_DataDefinitionConfiguration bb ON 
		aa.DataDefinitionNumber = bb.DataDefinitionNumber AND  
		UPPER(bb.DataDefinitionName) = 'BEG SAFE COUNT' 
	WHERE 
		COALESCE(aa.AllocationAmount, 0) &lt;&gt; 0) a 
JOIN (
	SELECT DATEADD(DAY, 1, aa.BusinessDate) AS BusinessDate, aa.SiteNumber, 
		aa.AllocationAmount
	FROM nbo.ST_Allocations aa 
	JOIN nbo.ST_DataDefinitionConfiguration bb ON 
		aa.DataDefinitionNumber = bb.DataDefinitionNumber AND  
		UPPER(bb.DataDefinitionName) = 'END SAFE COUNT' 
	WHERE 
		COALESCE(aa.AllocationAmount, 0) &lt;&gt; 0) b ON 
	a.BusinessDate = b.BusinessDate AND 
	a.SiteNumber = b.SiteNumber 
WHERE 
	a.AllocationAmount &lt;&gt; b.AllocationAmount AND 
	EXISTS ( 
		SELECT 1 
		FROM config.ProcessConfiguration z 
		JOIN nbo.ST_Sites y ON 
			CAST(z.ConfigurationValue AS NVARCHAR(25)) = y.StoreID
		WHERE  
			a.SiteNumber = y.SiteNumber AND 
			a.BusinessDate &gt;= CAST(z.EffectiveDate AS DATETIME) AND 
			a.BusinessDate &lt;= CAST(z.ObsoleteDate AS DATETIME) AND 
			z.ConfigurationName = 'Fusion' AND 
			z.ConfigurationCode = 'Inclusion')</property>
