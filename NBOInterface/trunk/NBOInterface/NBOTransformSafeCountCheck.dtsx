&#xA;ORDER BY a.EmailResultSetKey &#xA;&#xA;&#xA;--DROP TEMP TABLES &#xA;IF OBJECT_ID('tempdb..#EmailResultSet') IS NOT NULL &#xA;&#x9;DROP TABLE #EmailResultSet "
