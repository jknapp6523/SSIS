              description="OLE DB Source"
              name="ST_Items"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT CAST(CAST(GETDATE() AS DATE) AS DATETIME) AS BusinessDate, 
	N'0' AS StoreID, 
	CASE 
		WHEN (
			EXISTS ( 
				SELECT 1 
				FROM nbo.ST_VendorItems x 
				JOIN nbo.ST_Vendors w ON 
					x.SiteNumber = w.SiteNumber AND 
					x.VendorNumber = w.VendorNumber AND 			
					UPPER(w.VendorID) = 'DUMMY VENDOR' AND 
					UPPER(w.VendorName) = 'DUMMY VENDOR' 
				WHERE 
					a.ItemNumber = x.ItemNumber)) 
		THEN (CAST('IncorrectDummyVendorItemCode' AS VARCHAR(100))) 
		ELSE (CAST('MissingDummyVendorItems' AS VARCHAR(100))) 
	END AS DataCheck, a.ItemNumber AS NumericValue, 
	CAST(a.ItemID + ' - ' + a.ItemName AS NVARCHAR(240)) AS TextValue, 
	'Email' AS NotificationMethod 
FROM nbo.ST_Items a 
WHERE
	EXISTS ( 
		SELECT 1
		FROM nbo.ST_ItemCategories z
		JOIN config.ProcessConfiguration y ON 
			z.ItemCategoryID = CAST(y.ConfigurationValue AS NVARCHAR(25)) AND 
			y.ConfigurationName = 'InitialPricingInvoices' AND 
			y.ConfigurationCode = 'CategoryID' 
		WHERE 
			a.ItemCategoryNumber = z.ItemCategoryNumber) AND 
	NOT EXISTS ( 
		SELECT 1 
		FROM nbo.ST_VendorItems x 
		JOIN nbo.ST_Vendors w ON 
			x.SiteNumber = w.SiteNumber AND 
			x.VendorNumber = w.VendorNumber AND 			
			UPPER(w.VendorID) = 'DUMMY VENDOR' AND 
			UPPER(w.VendorName) = 'DUMMY VENDOR' 
		WHERE 
			CAST(x.ItemNumber AS NVARCHAR(50)) = x.VendorItemCode AND 
			a.ItemNumber = x.ItemNumber) AND 
	EXISTS ( 
		SELECT 1 
		FROM nbo.ST_VendorItems v 
		WHERE 
			a.ItemNumber = v.ItemNumber AND 
			v.OrderUnitPrice &gt; 0 AND 
			v.Exclude = 0)</property>
