          SQLTask:Connection="{90D0BD06-3AD3-497A-BD09-CCEE0343B8AA}"
          SQLTask:SqlStatementSource="---------------------------------------------------------------------------------------------------&#xA;-- INITIALIZE &#xA;--------------------------------------------------------------------------------------------------- &#xA;&#xA;--DECLARE AND SET&#xA;DECLARE @JobName NVARCHAR(256) &#xA;DECLARE @PackageName NVARCHAR(256) &#xA;DECLARE @AuditKey INT &#xA;DECLARE @SessionRequestedSource INT &#xA;&#xA;DECLARE @FileTimestamp VARCHAR(4000) &#xA;&#xA;DECLARE @FileOutput VARCHAR(4000) &#xA;DECLARE @FolderArchive VARCHAR(4000) &#xA;DECLARE @EmailDistro VARCHAR(4000) &#xA;&#xA;SET @JobName = ?  &#xA;SET @PackageName = ? &#xA;&#xA;SET @AuditKey = (&#xA;&#x9;SELECT MAX(a.AuditKey) AS AuditKey &#xA;&#x9;FROM audit.Session a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.JobName = @JobName AND &#xA;&#x9;&#x9;a.IsCurrentSession = 1) &#xA;&#xA;SET @SessionRequestedSource = ( &#xA;&#x9;SELECT a.SessionRequestedSource &#xA;&#x9;FROM audit.Session a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.AuditKey = @AuditKey) &#xA;&#xA;SET @FileTimestamp = CONVERT(VARCHAR(4000), GETDATE(), 120) &#xA;SET @FileTimestamp = REPLACE(@FileTimeStamp, '-', '') &#xA;SET @FileTimestamp = REPLACE(@FileTimeStamp, ' ', '') &#xA;SET @FileTimestamp = REPLACE(@FileTimeStamp, ':', '') &#xA;&#xA;SET @EmailDistro = ( &#xA;&#x9;SELECT a.ConfiguredValue&#xA;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;WHERE &#xA;&#x9;&#x9;a.ConfigurationFilter = 'Notification ' + @PackageName + ' Distro') &#xA;&#xA;IF @SessionRequestedSource = 1 &#xA;&#x9;BEGIN &#xA;&#xA;&#x9;&#x9;SET @FileOutput = ( &#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue + &#xA;&#x9;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;&#x9;WHEN (RIGHT(a.ConfiguredValue, 1) &lt;&gt; '\') THEN ('\') &#xA;&#x9;&#x9;&#x9;&#x9;ELSE ('') &#xA;&#x9;&#x9;&#x9;END + 'MissingDummyVendorItems_' + @FileTimestamp + '.csv'&#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder DataCheck Output') &#xA;&#xA;&#x9;&#x9;SET @FolderArchive = ( &#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue&#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder DataCheck Archive') &#xA;&#x9;END &#xA;ELSE &#xA;&#x9;BEGIN &#xA;&#xA;&#x9;&#x9;SET @FileOutput = ( &#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue + &#xA;&#x9;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;&#x9;WHEN (RIGHT(a.ConfiguredValue, 1) &lt;&gt; '\') THEN ('\') &#xA;&#x9;&#x9;&#x9;&#x9;ELSE ('') &#xA;&#x9;&#x9;&#x9;END + 'MissingDummyVendorItems_' + @FileTimestamp + '.csv'&#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder DataCheck Output Manual') &#xA;&#xA;&#x9;&#x9;SET @FolderArchive = ( &#xA;&#x9;&#x9;&#x9;SELECT a.ConfiguredValue&#xA;&#x9;&#x9;&#x9;FROM config.SSISConfigurations a &#xA;&#x9;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;&#x9;a.ConfigurationFilter = 'Folder DataCheck Archive Manual') &#xA;&#x9;END &#xA;&#xA;  &#xA;---------------------------------------------------------------------------------------------------&#xA;-- TERMINATE&#xA;---------------------------------------------------------------------------------------------------&#xA;&#xA;--RETURN &#xA;SELECT @AuditKey AS AuditKey, @FileOutput AS FileOutput, @FolderArchive AS FolderArchive, &#xA;&#x9;@EmailDistro AS EmailDistro &#xA;&#xA;"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
