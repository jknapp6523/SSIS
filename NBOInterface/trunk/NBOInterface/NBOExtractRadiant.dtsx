              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT a.DateOfBusiness, a.FKStoreId, a.FKEmployeeNumber, a.FKItemId, a.FKCategoryId, 
	SUM(a.Price) AS Price, SUM(a.Quantity) AS Quantity, SUM(a.DiscPric) AS DiscPric, 
	SUM(a.InclTax) AS InclTax, SUM(a.ExclTax) AS ExclTax
FROM dbo.dpvHstGndItem a 
JOIN dbo.Item b ON 
	a.FKItemId = b.ItemId AND 
	UPPER(b.LongName) = 'ENTERTAINMENT FEE'  
JOIN dbo.Category c ON 
	a.FKCategoryId = c.CategoryId AND 
	UPPER(c.Name) = 'MERCANTILE'
WHERE 
	a.DateOfBusiness BETWEEN ? AND ? 
GROUP BY a.DateOfBusiness, a.FKStoreId, a.FKEmployeeNumber, a.FKItemId, a.FKCategoryId</property>
