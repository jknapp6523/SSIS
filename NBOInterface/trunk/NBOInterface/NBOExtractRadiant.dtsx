        <SQLTask:SqlTaskData
          SQLTask:Connection="{9377DAFC-DFD6-4D45-8CAA-C053A85750F6}"
          SQLTask:SqlStatementSource="IF EXISTS (&#xA;&#x9;SELECT 1&#xA;&#x9;FROM sys.indexes a &#xA;&#x9;JOIN sys.objects b ON &#xA;&#x9;&#x9;a.object_id = b.object_id &#xA;&#x9;JOIN sys.schemas c ON &#xA;&#x9;&#x9;b.schema_id = c.schema_id &#xA;&#x9;WHERE &#xA;&#x9;&#x9;c.name + '.' + b.name = 'radiant.EmployeeByStore' AND &#xA;&#x9;&#x9;a.name = 'IDX_EmployeeByStore_EmployeeNumber')&#xA;DROP INDEX radiant.EmployeeByStore.IDX_EmployeeByStore_EmployeeNumber; &#xA;&#xA;CREATE NONCLUSTERED INDEX IDX_EmployeeByStore_EmployeeNumber  &#xA;ON radiant.EmployeeByStore (EmployeeNumber);   " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
