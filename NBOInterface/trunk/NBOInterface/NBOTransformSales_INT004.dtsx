                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @DateStart DATETIME 
DECLARE @DateEnd DATETIME 

SET @DateStart = ? 
SET @DateEnd = ? 

--DATA FROM NBO
SELECT a.SalesDate, a.StoreID, CONVERT(VARCHAR(10), a.SalesDate, 111) AS SalesDateStr, 
	COALESCE(ABS(a.PivotAmount), 0) AS PivotAmount, 
	CAST(SIGN(COALESCE(a.PivotAmount, 0)) AS SMALLINT) AS PivotAmountSign, 0 AS IsSentToFusion, 
	0 AS IsOffsettingEntry, a.PivotColumn 
FROM dbo.SalesUnpivot a 
WHERE 
	a.SalesDate &gt;= @DateStart AND 
	a.SalesDate &lt;= @DateEnd AND 
	EXISTS ( 
		SELECT 1 
		FROM config.ProcessConfiguration z 
		WHERE  
			a.StoreID = CAST(z.ConfigurationValue AS NVARCHAR(25)) AND 
			a.SalesDate &gt;= CAST(z.EffectiveDate AS DATETIME) AND 
			a.SalesDate &lt;= CAST(z.ObsoleteDate AS DATETIME) AND 
			z.ConfigurationName = 'Fusion' AND 
			z.ConfigurationCode = 'Inclusion')

	--REMOVE Other6 PER JASON BARCUS
	AND a.PivotColumn &lt;&gt; 'Other6' 

UNION ALL 

--ALL PREVIOUSLY PROCESSED DATA FOR OFFSETS 
SELECT DISTINCT a.SalesDate, a.StoreID, CONVERT(VARCHAR(10), a.SalesDate, 111) AS SalesDateStr, 
	CAST(0 AS MONEY) AS PivotAmount, CAST(0 AS SMALLINT) AS PivotAmountSign, 0 AS IsSentToFusion, 
	0 AS IsOffsettingEntry, a.PivotColumn
FROM dbo.SalesLookup a 
WHERE 
	a.PivotColumn NOT IN ('Deposit7_Credit', 'Deposit7_Debit') AND 
	EXISTS ( 
		SELECT 1 
		FROM config.ProcessConfiguration z 
		WHERE  
			a.StoreID = CAST(z.ConfigurationValue AS NVARCHAR(25)) AND 
			a.SalesDate &gt;= CAST(z.EffectiveDate AS DATETIME) AND 
			a.SalesDate &lt;= CAST(z.ObsoleteDate AS DATETIME) AND 
			z.ConfigurationName = 'Fusion' AND 
			z.ConfigurationCode = 'Inclusion')

	--REMOVE Other6 PER JASON BARCUS 
	AND a.PivotColumn &lt;&gt; 'Other6'</property>
