                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT a.DateOfBusiness AS SalesDate, CAST(a.FKStoreID AS NVARCHAR(25)) AS StoreID, 
	SUM(CAST(a.Price AS MONEY)) AS PivotAmount, 
	CAST('Gaming' AS NVARCHAR(255)) AS PivotColumn 
FROM radiant.dpvHstGndItem a 
JOIN radiant.Item b ON 
	a.FKItemId = b.ItemId AND 
	UPPER(b.LongName) = 'ENTERTAINMENT FEE'  
JOIN radiant.Category c ON 
	a.FKCategoryId = c.CategoryId AND 
	UPPER(c.Name) = 'MERCANTILE'
WHERE 
	a.DateOfBusiness BETWEEN ? AND ?   
GROUP BY a.DateOfBusiness, CAST(a.FKStoreID AS NVARCHAR(25))</property>
