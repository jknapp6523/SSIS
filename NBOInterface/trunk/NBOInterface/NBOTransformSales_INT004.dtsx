CT a.ConfiguredValue + &#xA;    CASE &#xA;     WHEN (RIGHT(a.ConfiguredValue, 1) = '\') THEN ('') &#xA;     ELSE ('\') &#xA;    END + 'GlInterfaceR11_' + @FileTimestamp + '_Error.csv' &#xA;   FROM config.SSISConfigurations a &#xA;   WHERE&#xA;    a.ConfigurationFilter = 'Folder Sales Error') &#xA;&#xA;&#xA;  --@FileOutput&#xA;  SET @FileOutput = (&#xA;   SELECT a.ConfiguredValue + &#xA;    CASE &#xA;     WHEN (RIGHT(a.ConfiguredValue, 1) = '\') THEN ('') &#xA;     ELSE ('\') &#xA;    END + 'GlInterfaceR11_' + @FileTimestamp + '.csv' &#xA;   FROM config.SSISConfigurations a &#xA;   WHERE&#xA;    a.ConfigurationFilter = 'Folder Sales Output') &#xA;&#xA;&#xA;  --@FileZipError&#xA;  SET @FileZipError = (&#xA;   SELECT a.ConfiguredValue + &#xA;    CASE &#xA;     WHEN (RIGHT(a.ConfiguredValue, 1) = '\') THEN ('') &#xA;     ELSE ('\') &#xA;    END + 'GlInterfaceR11_' + @FileTimestamp + '_Error.zip' &#xA;   FROM config.SSISConfigurations a &#xA;   WHERE&#xA;    a.ConfigurationFilter = 'Folder Sales Error')&#xA; &#xA;&#xA;  --@FileZipOutput&#xA;  SET @FileZipOutput = (&#xA;   SELECT a.ConfiguredValue + &#xA;    CASE &#xA;     WHEN (RIGHT(a.ConfiguredValue, 1) = '\') THEN ('') &#xA;     ELSE ('\') &#xA;    END + 'GlInterfaceR11_' + @FileTimestamp + '_fbdi.zip' &#xA;   FROM config.SSISConfigurations a &#xA;   WHERE&#xA;    a.ConfigurationFilter = 'Folder Sales Output')&#xA;&#xA;&#xA;  --@FolderArchive&#xA;  SET @FolderArchive = (&#xA;   SELECT a.ConfiguredValue + &#xA;    CASE &#xA;     WHEN (RIGHT(a.ConfiguredValue, 1) = '\') THEN ('') &#xA;     ELSE ('\') &#xA;    END  &#xA;   FROM config.SSISConfigurations a &#xA;   WHERE&#xA;    a.ConfigurationFilter = 'Folder Sales Archive')&#xA; END &#xA;ELSE &#xA; --NOT SCHEDULED (DEBUG OR MANUAL EXECUTION FROM AGENT) &#xA; BEGIN &#xA;&#xA;    --@FileError&#xA;  SET @FileError = (&#xA;   SELECT a.ConfiguredValue + &#xA;    CASE &#xA;     WHEN (RIGHT(a.ConfiguredValue, 1) = '\') THEN ('') &#xA;     ELSE ('\') &#xA;    END + 'GlInterfaceR