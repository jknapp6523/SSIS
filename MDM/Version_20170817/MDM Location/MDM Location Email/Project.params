  <SSIS:Parameter
    SSIS:Name="ExternalEmailBody">
    <SSIS:Properties>
      <SSIS:Property
        SSIS:Name="ID">{ffd04115-0465-48fa-b2fe-41010750bbc6}</SSIS:Property>
      <SSIS:Property
        SSIS:Name="CreationName"></SSIS:Property>
      <SSIS:Property
        SSIS:Name="Description"></SSIS:Property>
