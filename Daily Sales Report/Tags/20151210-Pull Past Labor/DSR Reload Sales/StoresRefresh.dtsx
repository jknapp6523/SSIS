                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT	CAST(s.StoreNum AS smallint) AS [StoreID],
		s.StoreName,
		s.DateOfOpen as [StoreOpenDate],
		s.Category as [StoreCategory],
		r.ManagerName,
		s.Region,
		CAST(RVP.RVP_ID AS VARCHAR) AS [RVPID],
		RVP.RVP_Name AS [RVPDesc]
FROM	StoreList s
		INNER JOIN Region r ON s.Region = r.Region
		INNER JOIN RVP ON s.RVP = RVP.RVP_ID
WHERE	s.Category IN ('O', 'F', 'N')
		AND s.DateOfOpen &lt;= GETDATE()
ORDER BY CONVERT(INT, StoreNum)</property>
