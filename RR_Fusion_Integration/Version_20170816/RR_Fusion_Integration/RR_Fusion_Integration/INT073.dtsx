            string sHeaderFileName = Dts.Variables["User::InputHeaderFileName"].Value.ToString();
            string sDetailFileName = sHeaderFileName.Replace("Header", "lines");
            
            string sLocalPath = Dts.Variables["User::NetworkInboxPath"].Value.ToString();
            if (!File.Exists((sLocalPath + sHeaderFileName)) || !File.Exists((sLocalPath + sDetailFileName)) || sHeaderFileName.Length != 31 || sDetailFileName.Length != 30)
