              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{45722FD3-1D3F-4BB4-9C13-604B78A11B20}"
                  SQLTask:SqlStatementSource="&#xA;UPDATE H SET H.IS_VALID = 0,&#xA;H.VALIDATION_ERROR_MESSAGE = 'NO_MATCH_IN_DETAIL'&#xA;FROM [Fusion].[FintechInvoiceHeaderStaging] H&#xA;LEFT JOIN [Fusion].[FintechInvoiceDetailStaging] D&#xA;ON D.InvoiceUniqueHeaderId=H.InvoiceUniqueId &#xA;WHERE D.InvoiceUniqueHeaderId IS NULL&#xA;AND H.IS_VALID = '1'&#xA;GO&#xA;&#xA;UPDATE D SET D.IS_VALID = 0,&#xA;D.VALIDATION_ERROR_MESSAGE = 'NO_MATCH_IN_HEADER'&#xA;FROM [Fusion].[FintechInvoiceHeaderStaging] H&#xA;RIGHT JOIN [Fusion].[FintechInvoiceDetailStaging] D&#xA;ON D.InvoiceUniqueHeaderId=H.InvoiceUniqueId&#xA;WHERE H.InvoiceUniqueId IS NULL&#xA;AND D.IS_VALID = '1'&#xA;GO&#xA;&#xA;&#xA;UPDATE D SET D.IS_VALID=0&#xA;&#x9;&#x9;    ,D.VALIDATION_ERROR_MESSAGE =CASE WHEN D.VALIDATION_ERROR_MESSAGE IS NULL THEN 'GLCODE_NOT_PRESENT'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  ELSE D.VALIDATION_ERROR_MESSAGE +'|GLCODE_NOT_PRESENT' END&#xA;FROM [Fusion].[FintechInvoiceDetailStaging] D&#xA;WHERE (D.GLCoding ='' OR D.GLCoding IS NULL)&#xA;GO&#xA;&#xA;UPDATE H SET H.IS_VALID = 0&#xA;&#x9;        ,H.VALIDATION_ERROR_MESSAGE =CASE WHEN H.VALIDATION_ERROR_MESSAGE IS NULL THEN 'NO_GL_CODING_PRESENT'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  ELSE H.VALIDATION_ERROR_MESSAGE +'|GLCODE_NOT_PRESENT' END&#xA;FROM [Fusion].[FintechInvoiceHeaderStaging] H&#xA;JOIN (SELECT DISTINCT InvoiceUniqueHeaderId  from [Fusion].[FintechInvoiceDetailStaging] WHERE (GLCoding ='' OR GLCoding IS NULL)) D&#xA;ON D.InvoiceUniqueHeaderId=H.InvoiceUniqueId &#xA;&#xA;GO&#xA;&#xA;UPDATE H SET H.IS_VALID=0&#xA;&#x9;&#x9;&#x9;,H.VALIDATION_ERROR_MESSAGE =CASE WHEN H.VALIDATION_ERROR_MESSAGE IS NULL THEN 'NO_MATCH_LOOKUP_VENDOR'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; ELSE H.VALIDATION_ERROR_MESSAGE +'|NO_MATCH_LOOKUP_VENDOR' END&#xA;FROM [Fusion].[FintechInvoiceHeaderStaging] H&#xA;LEFT JOIN [Fusion].[dimVendor] P&#xA;on H.VendorSiteId = Convert(BIGINT, RIGHT(P.FusionVendorSiteId, 7)) &#xA;WHERE P.FusionVendorNumber IS NULL&#xA;GO&#xA;&#xA;UPDATE H SET H.IS_VALID=0&#xA;&#x9;&#x9;&#x9;,H.VALIDATION_ERROR_MESSAGE = CASE WHEN H.VALIDATION_ERROR_MESSAGE IS NULL THEN 'NO_MATCH_LOOKUP_COMPANYCOSTCENTER'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  ELSE H.VALIDATION_ERROR_MESSAGE +'|NO_MATCH_LOOKUP_COMPANYCOSTCENTER' END&#xA;FROM [Fusion].[FintechInvoiceHeaderStaging] H&#xA;LEFT JOIN [Fusion].[dimCompanyCostCenter] C&#xA;on H.RestaurantNumber = C.RestaurantNumber &#xA;WHERE C.RestaurantNumber IS NULL&#xA;GO&#xA;&#xA;UPDATE D SET D.IS_VALID =H.IS_VALID &#xA;&#x9;&#x9;&#x9;,D.VALIDATION_ERROR_MESSAGE =CASE WHEN (D.VALIDATION_ERROR_MESSAGE IS NULL OR D.VALIDATION_ERROR_MESSAGE='GLCODE_NOT_PRESENT') THEN H.VALIDATION_ERROR_MESSAGE&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  ELSE D.VALIDATION_ERROR_MESSAGE +'|'+ H.VALIDATION_ERROR_MESSAGE END  &#xA;FROM [Fusion].[FintechInvoiceDetailStaging] D&#xA;INNER JOIN [Fusion].[FintechInvoiceHeaderStaging] H&#xA;on D.InvoiceUniqueHeaderId=H.InvoiceUniqueId&#xA;GO&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
              </DTS:ObjectData>
