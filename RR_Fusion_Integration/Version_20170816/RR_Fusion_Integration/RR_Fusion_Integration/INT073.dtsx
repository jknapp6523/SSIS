		public void Main()
		{
			// TODO: Add your code here
            //localFilePath = Dts.Variables["User::LocalFilePath"].Value.ToString();
            string NetworkOutboxPath = Dts.Variables["User::NetworkOutboxPath"].Value.ToString();
            string NetworkErrorPath = Dts.Variables["User::NetworkErrorPath"].Value.ToString();

            string invHeaderOutput = Dts.Variables["User::HeaderOutputFileName"].Value.ToString();
            string invDetailOutput = Dts.Variables["User::DetailOutputFileName"].Value.ToString();
            string invHeaderError = Dts.Variables["User::HeaderErrorFileName"].Value.ToString();
            string invDetailError = Dts.Variables["User::DetailErrorFileName"].Value.ToString();
            string invZipOutput = Dts.Variables["User::OutputZipFileName"].Value.ToString();
            string invZipError = Dts.Variables["User::ErrorZipFileName"].Value.ToString();

            int RowCntHeaderOutput = (int) Dts.Variables["User::RowCountHeaderValid"].Value;
            int RowCntDetailOutput = (int) Dts.Variables["User::RowCountDetailValid"].Value;
            int RowCntHeaderError = (int) Dts.Variables["User::RowCountHeaderError"].Value;
            int RowCntDetailError = (int) Dts.Variables["User::RowCountDetailError"].Value;

            //if (RowCntHeaderOutput > 0)
            //{
                if (File.Exists(NetworkOutboxPath + invZipOutput))
                {
                    File.Delete(NetworkOutboxPath + invZipOutput);
                }
                CompressFile(invZipOutput, invHeaderOutput, NetworkOutboxPath);
                
            //}

            //if (RowCntDetailOutput > 0)
            //{
                CompressFile(invZipOutput, invDetailOutput, NetworkOutboxPath);
            //}

            if (RowCntHeaderError > 0)
            {
                if (File.Exists(NetworkOutboxPath + invZipError))
                {
                    File.Delete(NetworkOutboxPath + invZipError);
                }
                CompressFile(invZipError, invHeaderError, NetworkErrorPath);
            }

            if (RowCntDetailError > 0)
            {
                CompressFile(invZipError, invDetailError, NetworkErrorPath);
            }

            Dts.TaskResult = (int)ScriptResults.Success;
		}

        private void CompressFile(string zipFile, string fileName,string NetworkFilePath)
        {
            string filePath = NetworkFilePath + fileName;
            zipFile = NetworkFilePath + zipFile;

            if (File.Exists(zipFile))
            {
                using (ZipArchive addfile = ZipFile.Open(zipFile, ZipArchiveMode.Update))
                {
                    addfile.CreateEntryFromFile(filePath, fileName);

                    //Delete the file after zipping it
                    File.Delete(filePath);
                }
            }
            else
            {
                var fileStream = new FileStream(zipFile, FileMode.Create);
                fileStream.Close();

                using (ZipArchive addfile = ZipFile.Open(zipFile, ZipArchiveMode.Update))
                {
                    addfile.CreateEntryFromFile(filePath, fileName);
                }

                //Delete the file after zipping it
                File.Delete(filePath);

            }
        }

        
        #region ScriptResults declaration
