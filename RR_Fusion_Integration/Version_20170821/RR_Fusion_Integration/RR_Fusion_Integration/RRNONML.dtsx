        /// </summary>
		public void Main()
		{
            string NetworkInboxPath = Dts.Variables["User::NetworkInboxPath"].Value.ToString();
            string NetworkArchivePath = Dts.Variables["User::NetworkArchivePath"].Value.ToString();
            string sFileMask = Dts.Variables["User::SourceFileMask"].Value.ToString();
            string sourceFileName = Dts.Variables["User::SourceFileName"].Value.ToString();
            string SrcFile = string.Empty;
            string DestFile = string.Empty;
            

            if (System.IO.Directory.Exists(NetworkInboxPath) && System.IO.Directory.Exists(NetworkArchivePath))
            {
                DirectoryInfo directoryInfo = new DirectoryInfo(NetworkInboxPath);
                FileInfo[] fileInfo = directoryInfo.GetFiles();
                DateTime lastModified = DateTime.MinValue;

                foreach (FileInfo file in fileInfo)
                {
                    if (file.Name.Contains(sFileMask))
                    {
                        SrcFile = System.IO.Path.Combine(NetworkInboxPath, file.Name);

                        DestFile = System.IO.Path.Combine(NetworkArchivePath, AppendTimeStamp(file.Name, sourceFileName));

                        if (File.Exists(DestFile))
                            File.Delete(DestFile);

                        File.Move(SrcFile, DestFile);
                    }
                }

            }

			Dts.TaskResult = (int)ScriptResults.Success;
		}


        public static string AppendTimeStamp(string fileName, string sourceFileName)
        {
            string sFileNameProcessed = string.Empty;
            string fileExt = string.Empty;

            if (fileName == sourceFileName)
                fileExt = Path.GetExtension(fileName);
            else
                fileExt = string.Concat("_Unprocessed", Path.GetExtension(fileName));

            return string.Concat(
                Path.GetFileNameWithoutExtension(fileName),"_",
                DateTime.Now.ToString("yyyyMMddHHmmssfff"),
                fileExt);
        }
        
        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>
