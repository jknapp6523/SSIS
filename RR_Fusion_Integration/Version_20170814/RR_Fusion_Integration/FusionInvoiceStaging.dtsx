		public void Main()
		{
            //string FileName = string.Empty;
            //string DestFile = string.Empty;
            string NetworkInboxPath = Dts.Variables["User::FIS_Inbox"].Value.ToString();
            string FileNameForamt = Dts.Variables["User::FIS_FileNameFormat"].Value.ToString();
           
            if (System.IO.Directory.Exists(NetworkInboxPath))
            {

                Dts.Variables["User::IsNetworkPathExists"].Value = true;

                string[] files = System.IO.Directory.GetFiles(NetworkInboxPath, FileNameForamt + "*.csv", System.IO.SearchOption.TopDirectoryOnly);

                if (files.Length > 0)
                {
                    Dts.Variables["User::IsSourceFilesExists"].Value = true;
                }
                else
                {
                    Dts.Variables["User::IsSourceFilesExists"].Value = false;
                }

            }

            else
            {
                Dts.Variables["User::IsNetworkPathExists"].Value = false;
            }

