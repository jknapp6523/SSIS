            string zipFile;
            string filePath;
            string networkFilePath;

            string OutputFile = Dts.Variables["User::OutputFileName"].Value.ToString();
            string ErrorFile = Dts.Variables["User::ErrorFileName"].Value.ToString();
            string OutputZipFile = Dts.Variables["User::OutputZipFileName"].Value.ToString();
            string ErrorZipFile = Dts.Variables["User::ErrorZipFileName"].Value.ToString();

            //Compress Output File
            networkFilePath = Dts.Variables["User::NetworkOutboxPath"].Value.ToString();
            filePath = networkFilePath + OutputFile;
            zipFile = networkFilePath + OutputZipFile;            
            CompressFile(zipFile, OutputFile, filePath);

            // Compress Error File
            networkFilePath = Dts.Variables["User::NetworkErrorPath"].Value.ToString();
            filePath = networkFilePath + ErrorFile;
            zipFile = networkFilePath + ErrorZipFile;           
            CompressFile(zipFile, ErrorFile, filePath);

            Dts.TaskResult = (int)ScriptResults.Success;
        }

        private void CompressFile(string zipFile, string fileName, string filePath)
        {
            if (File.Exists(zipFile))
            {
                //Delete existing zip file
                File.Delete(zipFile);
            }

             var fileStream = new FileStream(zipFile, FileMode.Create);
             fileStream.Close();

             using (ZipArchive addfile = ZipFile.Open(zipFile, ZipArchiveMode.Update))
             {
                addfile.CreateEntryFromFile(filePath, fileName);
             }

         }

