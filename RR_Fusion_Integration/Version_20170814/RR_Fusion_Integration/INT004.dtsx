xA;WHERE LookUpCol IN &#xA;(&#xA; 'OTHours',&#xA; 'LaborCost',&#xA; 'VoidSales',&#xA; 'CustomerCount',&#xA; 'Discount13'&#xA;)&#xA;&#xA;UPDATE Fusion.MLSalesLedgerStaging&#xA;SET Column52 = 'Check-Store Wells I'&#xA;WHERE Column11 = '11225' &#xA;AND LookupCol = ('Deposit11')&#xA;&#xA;UPDATE Fusion.MLSalesLedgerStaging&#xA;SET Column52 = 'Check-Store Wells II'&#xA;WHERE Column11 = '11230' &#xA;AND LookupCol = ('Deposit11')&#xA;&#xA;UPDATE Fusion.MLSalesLedgerStaging&#xA;SET Column52 = 'Check-Store Wells III'&#xA;WHERE Column11 = '11235' &#xA;AND LookupCol = ('Deposit11')&#xA;&#xA;UPDATE Fusion.MLSalesLedgerStaging&#xA;SET Column52 = 'Check-Store Wells IV'&#xA;WHERE Column11 = '11275' &#xA;AND LookupCol = ('Deposit11')&#xA;&#xA;UPDATE Fusion.MLSalesLedgerStaging&#xA;SET Column52 = 'Cash-Store Wells I'&#xA;WHERE Column11 = '11225' &#xA;AND LookupCol = ('ActualDeposit')&#xA;&#xA;UPDATE Fusion.MLSalesLedgerStaging&#xA;SET Column52 = 'Cash-Store Wells II'&#xA;WHERE Column11 = '11230' &#xA;AND LookupCol = ('ActualDeposit')&#xA;&#xA;UPDATE Fusion.MLSalesLedgerStaging&#xA;SET Column52 = 'Cash-Store Wells III'&#xA;WHERE Column11 = '11235' &#xA;AND LookupCol = ('ActualDeposit')&#xA;&#xA;UPDATE Fusion.MLSalesLedgerStaging&#xA;SET Column52 = 'Cash-Store Wells IV'&#xA;WHERE Column11 = '11275' &#xA;AND LookupCol = ('ActualDeposit')&#xA;&#xA;--New Rule with CR&#xA;UPDATE Fusion.MLSalesLedgerStaging&#xA;SET Column52 = 'Negative Cash Entry', &#xA;Column11 = '76210'&#xA;WHERE LookupCol = ('ActualDeposit') AND Amount &lt; 0&#xA;--End New Rule with CR&#xA;&#xA;DELETE FROM Fusion.MLSalesLedgerStaging&#xA;WHERE Amount = '0'" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
