    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{39C149BD-188B-4537-94BB-9734BBFE587A}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT top 1 [DateOfBusiness]&#xA;&#x9;,[FKStoreID]&#xA;&#x9;,(SELECT sum([Amount])&#xA;&#x9;&#x9;FROM [Radiant].[dbo].[dpvHstSalesSummary]&#xA;&#x9;&#x9;WHERE [Type] = 81&#xA;&#x9;&#x9;AND FKStoreId = &quot; + (DT_WSTR, 4) @[User::Restaurant] +&#xA;&#x9;&#x9;&quot; AND DateOfBusiness = &quot; + &quot;'&quot; +(DT_WSTR, 10) @[User::Yesterday] + &quot;'&quot; +&#xA;&#x9;&#x9;&quot; GROUP BY DateOfBusiness, FKStoreID) &#xA;AS TippableSales&#xA;&#x9;,(SELECT sum([Amount])&#xA;&#x9;&#x9;FROM [Radiant].[dbo].[dpvHstSalesSummary]&#xA;&#x9;&#x9;WHERE [Type] = 34&#xA;&#x9;&#x9;AND FKStoreId = &quot; + (DT_WSTR, 4) @[User::Restaurant] +&#xA;&#x9;&#x9;&quot; AND DateOfBusiness = &quot; + &quot;'&quot; + (DT_WSTR, 10) @[User::Yesterday] + &quot;'&quot; +&#xA;&#x9;&#x9;&quot; GROUP BY DateOfBusiness, FKStoreID) &#xA;AS Autogratuity&#xA;&#x9;,(SELECT sum([Amount])&#xA;&#x9;&#x9;FROM [Radiant].[dbo].[dpvHstSalesSummary]&#xA;&#x9;&#x9;WHERE [Type] = 2&#xA;&#x9;&#x9;AND FKStoreId = &quot; + (DT_WSTR, 4) @[User::Restaurant] +&#xA;&#x9;&#x9;&quot; AND DateOfBusiness = &quot; + &quot;'&quot; + (DT_WSTR, 10) @[User::Yesterday] + &quot;'&quot; +&#xA;&#x9;&#x9;&quot; GROUP BY DateOfBusiness, FKStoreID) &#xA;AS SalesTax&#xA;FROM [Radiant].[dbo].[dpvHstSalesSummary]&#xA;WHERE FKStoreID = &quot; + (DT_WSTR, 4) @[User::Restaurant] +&#xA;&quot; AND DateOfBusiness = &quot;+ &quot;'&quot; + (DT_WSTR, 10) @[User::Yesterday] + &quot;'&quot; +&#xA;&quot; GROUP BY [DateOfBusiness],[FKStoreID],[Type]&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SalesDateQuery">
      <DTS:VariableValue
        DTS:DataType="8">SELECT top 1 [DateOfBusiness]
	,[FKStoreID]
	,(SELECT sum([Amount])
		FROM [Radiant].[dbo].[dpvHstSalesSummary]
		WHERE [Type] = 81
		AND FKStoreId = 0 AND DateOfBusiness = '12/31/2012' GROUP BY DateOfBusiness, FKStoreID) 
AS TippableSales
	,(SELECT sum([Amount])
		FROM [Radiant].[dbo].[dpvHstSalesSummary]
		WHERE [Type] = 34
		AND FKStoreId = 0 AND DateOfBusiness = '12/31/2012' GROUP BY DateOfBusiness, FKStoreID) 
AS Autogratuity
	,(SELECT sum([Amount])
		FROM [Radiant].[dbo].[dpvHstSalesSummary]
		WHERE [Type] = 2
		AND FKStoreId = 0 AND DateOfBusiness = '12/31/2012' GROUP BY DateOfBusiness, FKStoreID) 
AS SalesTax
FROM [Radiant].[dbo].[dpvHstSalesSummary]
WHERE FKStoreID = 0 AND DateOfBusiness = '12/31/2012' GROUP BY [DateOfBusiness],[FKStoreID],[Type]</DTS:VariableValue>
    </DTS:Variable>
