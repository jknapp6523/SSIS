                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
	DATEADD(d, 0, DATEDIFF(d, 0, hvfa.ActivityDateTime)) AS [DateOfBusiness],
	hvfa.FKStoreID AS                                           [StoreID],
	hvfc.FKEmployeeID AS [EmployeeID],
	SUM(ISNULL(hvfc.CheckTotal, 0)) AS                      [RoyaltySalesTotal]
FROM  dbo.HstvbofqAssignment AS hvfa
INNER JOIN dbo.vbofqMemberAccount AS vfma
	 ON hvfa.FKvbofqMemberAccountID = vfma.vbofqMemberAccountID
INNER JOIN dbo.HstvbofqCheck AS hvfc
	 ON hvfa.HstvbofqAssignmentID = hvfc.FKHstvbofqAssignmentID
WHERE hvfa.[Status] = 4
	 AND LEFT(vfma.CardNumber, 5) IN
	('30384',
	 '30385'
	)
	 AND hvfa.FKStoreID != 0
	 AND DATEADD(d, 0, DATEDIFF(d, 0, hvfa.ActivityDateTime)) &gt;= DATEADD(d, -65, DATEDIFF(d, 0, GETDATE()))
                    AND DATEADD(d, 0, DATEDIFF(d, 0, hvfa.ActivityDateTime)) &lt; DATEADD(d, 0, DATEDIFF(d, 0, GETDATE()))
GROUP BY
	DATEADD(d, 0, DATEDIFF(d, 0, hvfa.ActivityDateTime)),
	hvfa.FKStoreID,
	hvfc.FKEmployeeID</property>
