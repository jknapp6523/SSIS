          SQLTask:SqlStatementSource="DELETE npsd&#xA;FROM [dbo].[NetPromoterScoreDetail] npsd&#xA;INNER JOIN [dbo].[NetPromoterScoreStaging] npss&#xA;&#x9;ON npsd.[VisitDate] = DATEADD(d, 0, DATEDIFF(d, 0, npss.[VisitDate]));&#xA;&#xA;INSERT INTO [dbo].[NetPromoterScoreDetail]&#xA;(&#xA;&#x9;[UID],&#xA;&#x9;[CouponNum],&#xA;&#x9;[StoreID],&#xA;&#x9;[DateOfBusiness],&#xA;&#x9;[MethodologyID],&#xA;&#x9;[Country],&#xA;&#x9;[VisitDate],&#xA;&#x9;[InviteType],&#xA;&#x9;[CheckNum],&#xA;&#x9;[EmployeeID],&#xA;&#x9;[EmployeeName],&#xA;&#x9;[NPS],&#xA;&#x9;[ServiceTime],&#xA;&#x9;[AttentiveServer],&#xA;&#x9;[TasteOfFood],&#xA;&#x9;[RestaurantCleanliness],&#xA;&#x9;[LongerSurvey],&#xA;&#x9;[Bottomless],&#xA;&#x9;[WOW],&#xA;&#x9;[OpenEnd],&#xA;&#x9;[Comment]&#xA;)&#xA;&#x9;  SELECT&#xA;&#x9;&#x9;  n.[UID],&#xA;&#x9;&#x9;  n.[CouponNum],&#xA;&#x9;&#x9;  n.[StoreID],&#xA;&#x9;&#x9;  n.[DateOfBusiness],&#xA;&#x9;&#x9;  n.[MethodologyID],&#xA;&#x9;&#x9;  n.[Country],&#xA;&#x9;&#x9;  DATEADD(d, 0, DATEDIFF(d, 0, n.[VisitDate])) AS [VisitDate],&#xA;&#x9;&#x9;  n.[InviteType],&#xA;&#x9;&#x9;  n.[CheckNum],&#xA;&#x9;&#x9;  n.[EmployeeID],&#xA;&#x9;&#x9;  n.[EmployeeName],&#xA;&#x9;&#x9;  n.[NPS],&#xA;&#x9;&#x9;  n.[ServiceTime],&#xA;&#x9;&#x9;  n.[AttentiveServer],&#xA;&#x9;&#x9;  n.[TasteOfFood],&#xA;&#x9;&#x9;  n.[RestaurantCleanliness],&#xA;&#x9;&#x9;  n.[LongerSurvey],&#xA;&#x9;&#x9;  n.[Bottomless],&#xA;&#x9;&#x9;  n.[WOW],&#xA;&#x9;&#x9;  n.[OpenEnd],&#xA;&#x9;&#x9;  n.[Comment]&#xA;&#x9;  FROM [dbo].[NetPromoterScoreStaging] AS n&#xA;&#x9;  WHERE n.[EmployeeID] IS NOT NULL OR n.[EmployeeID] &lt;&gt; ''" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
