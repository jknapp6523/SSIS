          SQLTask:SqlStatementSource="DELETE nps&#xA;FROM [dbo].[NetPromoterScore] nps&#xA;INNER JOIN [dbo].[NetPromoterScoreStaging] npss&#xA;     ON nps.DateOfBusiness = DATEADD(d, 0, DATEDIFF(d, 0, npss.VisitDate))&#xA;&#xA;INSERT INTO [dbo].[NetPromoterScore]&#xA;           ([DateOfBusiness]&#xA;           ,[StoreID]&#xA;           ,[EmployeeID]&#xA;           ,[Promoter]&#xA;           ,[Neutral]&#xA;           ,[Detractor])&#xA;SELECT&#xA;     DATEADD(d, 0, DATEDIFF(d, 0, n.VisitDate)) AS [DateOfBusiness],&#xA;     n.StoreID,&#xA;     n.EmployeeID,&#xA;     SUM(CASE WHEN n.NPS BETWEEN 9 AND 10&#xA;          THEN 1&#xA;          ELSE 0&#xA;     END) AS [Promoter],&#xA;     SUM(CASE WHEN n.NPS BETWEEN 7 AND 8&#xA;          THEN 1&#xA;          ELSE 0&#xA;     END) AS [Neutral],&#xA;     SUM(CASE WHEN n.NPS BETWEEN 0 AND 6&#xA;          THEN 1&#xA;          ELSE 0&#xA;     END) AS [Detractor]&#xA;FROM [dbo].[NetPromoterScoreStaging] n&#xA;GROUP BY&#xA;     DATEADD(d, 0, DATEDIFF(d, 0, n.VisitDate)),&#xA;     n.StoreID,&#xA;     n.EmployeeID" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
