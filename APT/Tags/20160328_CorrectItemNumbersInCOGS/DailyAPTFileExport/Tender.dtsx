                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SET NOCOUNT ON

	DECLARE @Hierarchy TABLE 
	(StoreID INT
	, Company VARCHAR(64)
	, UNIQUE CLUSTERED (StoreID, Company))

	INSERT INTO @Hierarchy
	SELECT DISTINCT StoreID, Company
	FROM 
	[HQ-FINSQL-01].[Staging].[dbo].[vwIndexed_Source_RegionalAlignment]


	DECLARE @BeginDate datetime 
	, @EndDate datetime

	SET @BeginDate =  DATEADD(day,-16,CONVERT(CHAR(10), GETDATE(), 111))
	SET @EndDate  = CONVERT(CHAR(10), GETDATE(), 111) 



	DECLARE @Result TABLE (UniqueID BIGINT, RestaurantID INT, DateOfBusiness DATETIME, CheckNumber INT,
		[Type] INT, TypeID INT, Name VARCHAR(100), Amount DECIMAL(12,4), [Hour] INT, [Minute] INT,
		FKEmployeeNumber INT, GuestCount DECIMAL(12,4))	

	INSERT INTO @Result 
	SELECT 
     t.[UniqueID]
	,t.[FKStoreId] as RestaurantID
	,t.[DateOfBusiness]
	,t.[CheckNumber]
	,t.[Type]
	,t.[TypeId] 
	,t.[Name] 
	,t.[Amount]
	,t.[Hour]
	,t.[Minute]
	,t.[FKEmployeeNumber]
	,0 AS [GuestCount]
	FROM [Radiant].[dbo].[dpvHstGndTender] t
	INNER JOIN @Hierarchy a
	ON t.FKStoreID = a.StoreID
	WHERE t.[DateOfBusiness] &gt;= DATEADD(day,-16,CONVERT(CHAR(10), GETDATE(), 111))
	and t.[DateOfBusiness] &lt; CONVERT(CHAR(10), GETDATE(), 111)
	AND a.Company IN ('RRGB','RRBW')
	ORDER BY  t.[DateOfBusiness] DESC, t.[FKStoreId], [CheckNumber], [Type], [TypeId]


	UPDATE @RESULT
	SET GuestCount = c.[GuestCount]
	FROM @RESULT r
	JOIN [Radiant].[dbo].[DpvHstCheckSummary] c
	ON r.RestaurantID=c.FkStoreID
	AND r.DateofBusiness=c.DateofBusiness
	and r.CheckNumber=c.checkid


	SELECT * FROM @RESULT
	ORDER BY DateOfBusiness DESC, RestaurantID, CheckNumber, [Type], [TypeID]</property>
