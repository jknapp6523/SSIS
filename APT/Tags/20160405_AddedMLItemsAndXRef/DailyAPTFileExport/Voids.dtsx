                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SET NOCOUNT ON

	DECLARE @Hierarchy TABLE 
	(StoreID INT
	, Company VARCHAR(64)
	, UNIQUE CLUSTERED (StoreID, Company))

	INSERT INTO @Hierarchy
	SELECT DISTINCT StoreID, Company
	FROM 
	[HQ-FINSQL-01].[Staging].[dbo].[vwIndexed_Source_RegionalAlignment]


	DECLARE @BeginDate datetime 
	, @EndDate datetime

	SET @BeginDate =  DATEADD(day,-16,CONVERT(CHAR(10), GETDATE(), 111))
	SET @EndDate  = CONVERT(CHAR(10), GETDATE(), 111) 
	
SELECT  [UniqueID]
,[FKStoreId] as RestaurantID
,[DateOfBusiness]
,[CheckNumber]
,[FKReasonId]
,[EntryId]
,[FKItemId]
,[Price]
,[Hour]
,[Minute]
,[Inventory]
FROM [Radiant].[dbo].[dpvHstGndVoid] v
INNER JOIN @Hierarchy a
ON v.FKStoreID = a.StoreID
where v.DateofBusiness &gt;= @BeginDate
and v.dateofbusiness &lt; @EndDate
AND a.Company IN ('RRGB','RRBW')
order by  v.dateofbusiness desc , v.fkstoreid , v.checknumber, v.entryid</property>
