    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{9BD4F213-705C-4D63-BDA2-6591F9FEDAFB}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;select L.StoreNum, &quot; + (DT_WSTR, 4)  @[User::SubPeriod] +&quot; SubPeriod, '&quot; + (DT_WSTR,32) &#xA;&#xA;&#xA; @[User::DateOfBusiness] + &quot;' PeriodEndDate, case isnull(B.StoreID, 1) when 1 then 1 else 0 end BeverageCostDataMissing,&#xA;&#xA;&#xA;case isnull(C.StoreID, 1) when 1 then 1 else 0 end CashAndSalesDataMissing,&#xA;&#xA;&#xA;case isnull(G.StoreID, 1) when 1 then 1 else 0 end CostOfGoodsDataMissing,&#xA;&#xA;&#xA;case when isnull(d.Cnt, 1) -  isnull(h.Cnt,1)  = 0 then 0 else 1 end InvoiceDetailsDataMissing,&#xA;&#xA;case  when isnull(d.Cnt, 1) - isnull(h.Cnt,1)  = 0 then 0 else 1 end InvoiceHeadersDataMissing,&#xA;&#xA;&#xA;case isnull(I.StoreID, 1) when 1 then 1 else 0 end ItemUseVarianceDataMissing,&#xA;&#xA;&#xA;case isnull(M.StoreID, 1) when 1 then 1 else 0 end MICDataMissing,&#xA;&#xA;&#xA;case isnull(V.StoreID, 1) when 1 then 1 else 0 end PandLCatDataMissing,&#xA;&#xA;&#xA;case isnull(W.StoreID, 1) when 1 then 1 else 0 end PandLPrctCatDataMissing,&#xA;&#xA;&#xA;case isnull(X.StoreID, 1) when 1 then 1 else 0 end PandLPrctGrpDataMissing,&#xA;&#xA;&#xA;case isnull(Y.StoreID, 1) when 1 then 1 else 0 end PandLPrctTotDataMissing,&#xA;&#xA;&#xA;case isnull(SG.StoreID, 1) when 1 then 1 else 0 end SalesGuestsByHalfHourDataMissing,&#xA;&#xA;&#xA;case isnull(SM.StoreID, 1) when 1 then 1 else 0 end SalesMixDataMissing&#xA;&#xA;&#xA;&#xA;from&#xA; MLAddOn.dbo.StoreList L&#xA;&#xA;&#xA;left join ##B B on B.StoreID = L.StoreNum&#xA;left join ##C C on C.StoreID = L.StoreNum&#xA;left join ##G G on G.StoreID = L.StoreNum&#xA;left join ##D D on D.StoreID = L.StoreNum&#xA;left join ##H H on H.StoreID = L.StoreNum&#xA;left join ##I I on I.StoreID = L.StoreNum&#xA;left join ##M M on M.StoreID = L.StoreNum&#xA;left join ##V V on V.StoreID = L.StoreNum&#xA;left join ##W W on W.StoreID = L.StoreNum&#xA;left join ##X X on X.StoreID = L.StoreNum&#xA;left join ##Y Y on Y.StoreID = L.StoreNum&#xA;left join ##SG SG on SG.StoreID = L.StoreNum&#xA;left join ##SM SM on SM.StoreID = L.StoreNum&#xA;&#xA;&#xA;&#xA;&#xA;where L.Category in ('O', 'F', 'P')&#xA;&#xA;   and DateOfOpen &lt;= '&quot; + (DT_WSTR, 32) @[User::DateOfBusiness] + &quot;'&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="ST_SourceQuery">
      <DTS:VariableValue
        DTS:DataType="8">select L.StoreNum, 0 SubPeriod, '12/3/2015' PeriodEndDate, case isnull(B.StoreID, 1) when 1 then 1 else 0 end BeverageCostDataMissing,


case isnull(C.StoreID, 1) when 1 then 1 else 0 end CashAndSalesDataMissing,


case isnull(G.StoreID, 1) when 1 then 1 else 0 end CostOfGoodsDataMissing,


case when isnull(d.Cnt, 1) -  isnull(h.Cnt,1)  = 0 then 0 else 1 end InvoiceDetailsDataMissing,

case  when isnull(d.Cnt, 1) - isnull(h.Cnt,1)  = 0 then 0 else 1 end InvoiceHeadersDataMissing,


case isnull(I.StoreID, 1) when 1 then 1 else 0 end ItemUseVarianceDataMissing,


case isnull(M.StoreID, 1) when 1 then 1 else 0 end MICDataMissing,


case isnull(V.StoreID, 1) when 1 then 1 else 0 end PandLCatDataMissing,


case isnull(W.StoreID, 1) when 1 then 1 else 0 end PandLPrctCatDataMissing,


case isnull(X.StoreID, 1) when 1 then 1 else 0 end PandLPrctGrpDataMissing,


case isnull(Y.StoreID, 1) when 1 then 1 else 0 end PandLPrctTotDataMissing,


case isnull(SG.StoreID, 1) when 1 then 1 else 0 end SalesGuestsByHalfHourDataMissing,


case isnull(SM.StoreID, 1) when 1 then 1 else 0 end SalesMixDataMissing



from
 MLAddOn.dbo.StoreList L


left join ##B B on B.StoreID = L.StoreNum
left join ##C C on C.StoreID = L.StoreNum
left join ##G G on G.StoreID = L.StoreNum
left join ##D D on D.StoreID = L.StoreNum
left join ##H H on H.StoreID = L.StoreNum
left join ##I I on I.StoreID = L.StoreNum
left join ##M M on M.StoreID = L.StoreNum
left join ##V V on V.StoreID = L.StoreNum
left join ##W W on W.StoreID = L.StoreNum
left join ##X X on X.StoreID = L.StoreNum
left join ##Y Y on Y.StoreID = L.StoreNum
left join ##SG SG on SG.StoreID = L.StoreNum
left join ##SM SM on SM.StoreID = L.StoreNum




where L.Category in ('O', 'F', 'P')

   and DateOfOpen &lt;= '12/3/2015'</DTS:VariableValue>
    </DTS:Variable>
