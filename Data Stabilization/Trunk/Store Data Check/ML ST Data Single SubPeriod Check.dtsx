DT_WSTR, 4) @[User::FiscalYear] + &#xA;          &quot;  and PercentAmountWeek&quot; + (DT_WSTR, 1) @[User::WeekNumber] + &quot; &gt; 0;&#xA;&#xA;select distinct StoreID into ##Y&#xA;from [dbo].[ST_PandLPrctTot]&#xA;where ReportingPeriod# = &quot; + (DT_WSTR, 2) @[User::ReportingPeriod] + &#xA;          &quot;  and fiscalyear = &quot; + (DT_WSTR, 4) @[User::FiscalYear] + &#xA;          &quot;  and PercentAmountWeek&quot; + (DT_WSTR, 1) @[User::WeekNumber] + &quot; &gt; 0;&#xA;&#xA;select distinct StoreID into ##SG&#xA;from [dbo].[ST_SalesGuestsByHalfHour]&#xA;where subperiod= &quot; + (DT_WSTR, 4)  @[User::SubPeriod] +&quot;;&#xA;&#xA;select distinct StoreID into ##SM&#xA;from [dbo].[ST_SalesMix]&#xA; where salesmixdate= '&quot; + (DT_WSTR, 32) @[User::DateOfBusiness] + &quot;'&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="TempTables">
      <DTS:VariableValue
        DTS:DataType="8">select distinct StoreID into ##B
from ST_BeverageCost
where SubPeriod# = 0;

select distinct StoreID into ##C
from [dbo].[ST_CashAndSales]
where SubPeriod# = 0;

select distinct StoreID into ##G
from [dbo].[ST_CostOfGoods]
where SubPeriod = 0;

select StoreID, count(distinct invoiceid) Cnt into ##D
from [dbo].[ST_InvoiceDetails]
where SubPeriod = 0 group by storeid;

select  StoreID, count(distinct invoiceid) Cnt into ##H
from [dbo].[ST_InvoiceHeaders]
where SubPeriod = 0 group by storeid;

select distinct StoreID into ##I
from [dbo].[ST_ItemUseVariance]
where SubPeriod = 0;

select distinct StoreID into ##M
from [dbo].[ST_MIC]
where SubPeriod# = 0;

select distinct StoreID into ##V
from [dbo].[ST_PandLCat]
where ReportingPeriod# = 0 and fiscalyear = 0	and AmountWeek1 &gt; 0;

select distinct StoreID into ##W
from [dbo].[ST_PandLPrctCat]
where ReportingPeriod# = 0  and fiscalyear = 0  and PercentAmountWeek1 &gt; 0;

select distinct StoreID into ##X
from [dbo].[ST_PandLPrctGrp]
where ReportingPeriod# = 0  and fiscalyear = 0  and PercentAmountWeek1 &gt; 0;

select distinct StoreID into ##Y
from [dbo].[ST_PandLPrctTot]
where ReportingPeriod# = 0  and fiscalyear = 0  and PercentAmountWeek1 &gt; 0;

select distinct StoreID into ##SG
from [dbo].[ST_SalesGuestsByHalfHour]
where subperiod= 0;

select distinct StoreID into ##SM
from [dbo].[ST_SalesMix]
 where salesmixdate= '12/3/2015'</DTS:VariableValue>
